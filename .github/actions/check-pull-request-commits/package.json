{
    "name": "check-pull-request-commits",
    "version": "1.0.0",
    "description": "Simple GitHub action that checks pull request commits",
    "author": "GoodData",
    "license": "MIT",
    "scripts": {
        "build": "parcel build src/index.js"
    },
    "targets": {
        "default": {
            "context": "node",
            "includeNodeModules": true,
            "distDir": "action-build",
            "outputFormat": "commonjs"
        }
    },
    "type": "commonjs",
    "dependencies": {
        "@actions/core": "^1.10.0",
        "@commitlint/cli": "^17.6.1",
        "@commitlint/config-conventional": "^17.6.1",
        "@commitlint/format": "^17.4.4",
        "@commitlint/lint": "^17.6.1",
        "@commitlint/load": "^17.5.0",
        "@commitlint/read": "^17.5.1"
    },
    "devDependencies": {
        "parcel": "^2.12.0"
    }
}
