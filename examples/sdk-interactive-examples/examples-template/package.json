{
    "name": "@gooddata/sdk-interactive-examples-template",
    "version": "11.19.0-alpha.11",
    "private": false,
    "description": "GoodData interactive example template",
    "license": "LicenseRef-LICENSE",
    "author": "GoodData",
    "type": "module",
    "scripts": {
        "_phase:build": "npm run build",
        "_phase:validate": "npm run validate",
        "build": "npm-run-all -p build-check build-ts",
        "build-check": "tsgo",
        "build-ts": "tsgo -p tsconfig.build.json",
        "build-example": "vite build",
        "clean": "rm -rf esm dist *.log",
        "eslint": "eslint src",
        "eslint-fix": "eslint src --fix",
        "oxfmt-check": "oxfmt --check .",
        "oxfmt-write": "oxfmt .",
        "preview": "vite preview",
        "refresh-md": "gdc-catalog-export && npm run oxfmt-write",
        "start": "vite serve",
        "update-version": "node ./scripts/updateWorkspaceVersion.mjs",
        "validate": "npm run lint && npm run oxfmt-check",
        "lint": "npm run oxlint && npm run eslint",
        "lint-fix": "npm run oxlint-fix && npm run eslint-fix",
        "oxlint": "oxlint .",
        "oxlint-fix": "oxlint . --fix"
    },
    "dependencies": {
        "@gooddata/sdk-backend-base": "workspace:*",
        "@gooddata/sdk-backend-spi": "workspace:*",
        "@gooddata/sdk-backend-tiger": "workspace:*",
        "@gooddata/sdk-model": "workspace:*",
        "@gooddata/sdk-ui": "workspace:*",
        "@gooddata/sdk-ui-charts": "workspace:*",
        "@gooddata/sdk-ui-dashboard": "workspace:*",
        "@gooddata/sdk-ui-ext": "workspace:*",
        "@gooddata/sdk-ui-filters": "workspace:*",
        "@gooddata/sdk-ui-geo": "workspace:*",
        "@gooddata/sdk-ui-kit": "workspace:*",
        "@gooddata/sdk-ui-pivot": "workspace:*",
        "react": "19.1.1",
        "react-dom": "19.1.1",
        "tslib": "2.8.1"
    },
    "devDependencies": {
        "@gooddata/catalog-export": "workspace:*",
        "@gooddata/eslint-config": "workspace:*",
        "@gooddata/oxlint-config": "workspace:*",
        "@types/node": "^22.13.0",
        "@types/react": "19.1.11",
        "@types/react-dom": "19.1.7",
        "@typescript-eslint/eslint-plugin": "8.52.0",
        "@typescript-eslint/parser": "8.52.0",
        "@typescript/native-preview": "7.0.0-dev.20260202.1",
        "@vitejs/plugin-react": "^5.1.2",
        "eslint": "^9.39.2",
        "eslint-import-resolver-typescript": "4.4.4",
        "eslint-plugin-headers": "1.3.3",
        "eslint-plugin-import-esm": "1.2.1",
        "eslint-plugin-import-x": "4.16.1",
        "eslint-plugin-jsdoc": "62.1.0",
        "eslint-plugin-no-barrel-files": "1.2.2",
        "eslint-plugin-react": "7.37.5",
        "eslint-plugin-react-hooks": "5.2.0",
        "eslint-plugin-sonarjs": "3.0.6",
        "npm-run-all": "^4.1.5",
        "oxfmt": "0.27.0",
        "oxlint": "^1.43.0",
        "process": "^0.11.10",
        "rolldown": "1.0.0-rc.1",
        "typescript": "5.9.3",
        "vite": "8.0.0-beta.10",
        "vite-plugin-html": "~3.2.0"
    },
    "exampleDependencies": [],
    "gooddata": {
        "hostname": "https://public-examples.gooddata.com",
        "workspaceId": "demo_ecommerce",
        "backend": "tiger",
        "catalogOutput": "src/catalog.ts"
    },
    "title": "GoodData interactive example template"
}
