{
    "extends": "../../tsconfig.strict.json",
    "compilerOptions": {
        "target": "ES2022",
        "lib": ["ES2022", "dom"],
        "module": "NodeNext",
        "moduleResolution": "NodeNext",
        "jsx": "react",
        "importHelpers": true,
        "incremental": false,
        "resolveJsonModule": true,
        "declaration": true,
        "declarationMap": true,
        "sourceMap": true,
        "strictNullChecks": false,
        "exactOptionalPropertyTypes": false
    }
}
