{
    "name": "@gooddata/sdk-ui-kit",
    "version": "8.9.0-beta.0",
    "description": "GoodData SDK - UI Building Components",
    "repository": {
        "type": "git",
        "url": "https://github.com/gooddata/gooddata-ui-sdk.git",
        "directory": "libs/sdk-ui-kit"
    },
    "author": "GoodData Corporation",
    "license": "MIT",
    "main": "dist/index.js",
    "module": "esm/index.js",
    "es2015": "esm/index.js",
    "browser": "dist/index.js",
    "typings": "esm/index.d.ts",
    "sideEffects": [
        "*.css",
        "*.svg",
        "*.ttf",
        "*.woff",
        "*.eot"
    ],
    "files": [
        "dist/**/*.js",
        "dist/**/*.json",
        "dist/**/*.d.ts",
        "dist/**/*.map",
        "dist/**/*.svg",
        "esm/**/*.js",
        "esm/**/*.json",
        "esm/**/*.d.ts",
        "esm/**/*.map",
        "esm/**/*.svg",
        "styles"
    ],
    "config": {
        "eslint": "-c .eslintrc.js --ext ts,tsx src/"
    },
    "scripts": {
        "clean": "rm -rf ci dist esm coverage styles/css *.log && jest --clearCache",
        "api-extractor": "mkdir -p api && [ -z \"${CI}\" ] && (api-extractor run -l) || (api-extractor run)",
        "build": "bash scripts/build.sh && npm run api-extractor",
        "build-dev": "bash scripts/build.sh --dev",
        "build-cjs": "tsc -p tsconfig.build.json",
        "build-esm": "tsc -p tsconfig.build.esm.json",
        "build-styles": "bash scripts/build.sh --styles",
        "build-incremental": "concurrently \"npm run build-styles\" \"npm run build-esm\"",
        "dev": "bash scripts/build.sh --dev-watch",
        "test": "jest --watch",
        "test-once": "jest --maxWorkers=${JEST_MAX_WORKERS:-'45%'}",
        "test-ci": "JEST_JUNIT_OUTPUT=./ci/results/test-results.xml jest --ci --config jest.ci.js",
        "eslint": "eslint $npm_package_config_eslint",
        "eslint-ci": "mkdir -p ./ci/results && eslint -f checkstyle -o ci/results/eslint-results.xml $npm_package_config_eslint",
        "prettier-check": "prettier --check '{src,styles,__mocks__}/**/*.{ts,tsx,json,scss,md,yaml,html}'",
        "prettier-write": "prettier --write '{src,styles,__mocks__}/**/*.{ts,tsx,json,scss,md,yaml,html}'",
        "dep-cruiser": "depcruise --validate .dependency-cruiser.js --output-type err-long src/",
        "dep-cruiser-ci": "depcruise --validate .dependency-cruiser.js --output-type err-long src/",
        "stylelint": "stylelint '**/*.scss'",
        "stylelint-ci": "stylelint '**/*.scss' --custom-formatter=node_modules/stylelint-checkstyle-formatter > ./ci/results/stylelint-results.xml",
        "validate": "npm run dep-cruiser && npm run eslint && npm run stylelint && npm run prettier-check",
        "validate-ci": "npm run dep-cruiser-ci && npm run eslint-ci && npm run stylelint-ci && npm run prettier-check"
    },
    "dependencies": {
        "@aaronhayes/react-use-hubspot-form": "^2.0.1",
        "@gooddata/numberjs": "^4.0.1",
        "@gooddata/sdk-backend-spi": "^8.9.0-beta.0",
        "@gooddata/sdk-model": "^8.9.0-beta.0",
        "@gooddata/sdk-ui": "^8.9.0-beta.0",
        "@gooddata/sdk-ui-theme-provider": "^8.9.0-beta.0",
        "@gooddata/util": "^8.9.0-beta.0",
        "classnames": "^2.3.1",
        "codemirror": "^5.54.0",
        "date-fns": "^2.22.1",
        "debounce-promise": "^3.1.2",
        "element-closest-polyfill": "^1.0.0",
        "fixed-data-table-2": "^1.2.0",
        "foundation-sites": "^5.5.3",
        "lodash": "^4.17.19",
        "moment-timezone": "^0.5.33",
        "moment": "^2.24.0",
        "react-codemirror2": "^7.2.0",
        "react-day-picker": "^7.4.10",
        "react-helmet": "^6.1.0",
        "react-intl": "^5.23.0",
        "react-native-listener": "~1.1.0",
        "react-portal": "^4.1.5",
        "react-responsive": "^8.2.0",
        "react-select": "^3.1.0",
        "react-textarea-autosize": "^8.3.2",
        "react-transition-group": "^4.4.2",
        "tinycolor2": "^1.4.2",
        "ts-invariant": "^0.7.3",
        "tslib": "^2.0.0",
        "uuid": "^8.3.2",
        "@types/node": "^16.11.11"
    },
    "peerDependencies": {
        "react": "^16.10.0 || ^17.0.0",
        "react-dom": "^16.10.0 || ^17.0.0"
    },
    "devDependencies": {
        "@gooddata/eslint-config": "^2.1.0",
        "@gooddata/reference-workspace": "^8.9.0-beta.0",
        "@gooddata/sdk-backend-mockingbird": "^8.9.0-beta.0",
        "@gooddata/stylelint-config": "^5.0.0",
        "@microsoft/api-documenter": "^7.12.13",
        "@microsoft/api-extractor": "^7.18.4",
        "@types/codemirror": "^0.0.95",
        "@types/debounce-promise": "^3.1.4",
        "@types/enzyme": "^3.10.3",
        "@types/jest": "^27.0.1",
        "@types/kefir": "^3.8.5",
        "@types/lodash": "^4.14.158",
        "@types/raf": "^3.4.0",
        "@types/react-dom": "^17.0.11",
        "@types/react-helmet": "^6.1.4",
        "@types/react-portal": "^4.0.2",
        "@types/react-responsive": "^8.0.2",
        "@types/react-select": "^3.0.12",
        "@types/react-transition-group": "^4.4.4",
        "@types/react": "^17.0.34",
        "@types/tinycolor2": "^1.4.2",
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/eslint-plugin": "^5.5.0",
        "@typescript-eslint/parser": "^5.5.0",
        "@wojtekmaj/enzyme-adapter-react-17": "^0.6.5",
        "concurrently": "^6.0.2",
        "dependency-cruiser": "^10.1.1",
        "enzyme": "^3.10.0",
        "eslint-plugin-header": "^3.0.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-jest": "^25.3.0",
        "eslint-plugin-no-only-tests": "^2.4.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react-hooks": "^4.0.8",
        "eslint-plugin-react": "^7.20.5",
        "eslint-plugin-sonarjs": "^0.11.0",
        "eslint": "^8.3.0",
        "jest": "^27.4.1",
        "jest-enzyme": "^7.1.2",
        "jest-junit": "^3.0.0",
        "kefir": "^3.8.8",
        "node-sass-magic-importer": "^5.3.2",
        "node-sass": "^6.0.1",
        "prettier": "~2.5.0",
        "raf": "^3.4.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "stylelint-checkstyle-formatter": "^0.1.2",
        "stylelint-config-prettier": "^8.0.2",
        "stylelint": "^13.8.0",
        "svgo": "^2.8.0",
        "ts-jest": "^27.0.5",
        "typescript": "4.0.2",
        "eslint-plugin-tsdoc": "^0.2.14"
    }
}
