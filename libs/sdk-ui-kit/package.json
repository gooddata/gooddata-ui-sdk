{
    "name": "@gooddata/sdk-ui-kit",
    "version": "11.5.0-alpha.1",
    "description": "GoodData SDK - UI Building Components",
    "repository": {
        "type": "git",
        "url": "https://github.com/gooddata/gooddata-ui-sdk.git",
        "directory": "libs/sdk-ui-kit"
    },
    "license": "MIT",
    "author": "GoodData Corporation",
    "sideEffects": [
        "*.css",
        "*.svg",
        "*.ttf",
        "*.woff",
        "*.eot"
    ],
    "type": "module",
    "exports": {
        ".": "./esm/index.js",
        "./styles/*": "./styles/*",
        "./esm/assets/*": "./esm/assets/*",
        "./utils/*": "./esm/utils/*.js",
        "./src/*": "./src/*"
    },
    "browser": "./esm/index.js",
    "types": "./esm/index.d.ts",
    "files": [
        "esm/**/*.js",
        "esm/**/*.json",
        "esm/**/*.d.ts",
        "esm/**/*.map",
        "esm/**/*.svg",
        "src/**/*.scss",
        "styles"
    ],
    "scripts": {
        "_phase:build": "npm run build",
        "_phase:test": "npm run test-once",
        "_phase:validate": "npm run validate",
        "api-extractor": "mkdir -p api && [ -z \"${CI}\" ] && (api-extractor run -l) || (api-extractor run)",
        "build": "bash scripts/build.sh",
        "clean": "../../common/scripts/clean-command-state.sh && rm -rf ci dist esm coverage styles/css *.log tsconfig.tsbuildinfo",
        "dep-cruiser": "depcruise --validate .dependency-cruiser.cjs --output-type err-long src/",
        "dep-cruiser-ci": "depcruise --validate .dependency-cruiser.cjs --output-type err-long src/",
        "eslint": "eslint -c .eslintrc.cjs src/",
        "eslint-ci": "mkdir -p ./ci/results && eslint -f checkstyle -o ci/results/eslint-results.xml -c .eslintrc.cjs src/",
        "eslint-fix": "eslint -c .eslintrc.cjs src/ --fix",
        "generate-theme": "node scripts/generateDefaultThemeScss.js && prettier --write ./src/@ui/defaultTheme.scss",
        "prettier-check": "prettier --check '{src,styles,__mocks__}/**/*.{ts,tsx,json,scss,md,yaml,html}'",
        "prettier-write": "prettier --write '{src,styles,__mocks__}/**/*.{ts,tsx,json,scss,md,yaml,html}'",
        "scss": "sass --load-path=node_modules --load-path=node_modules/fixed-data-table-2/dist styles/scss:styles/css",
        "stylelint": "stylelint '**/*.scss'",
        "stylelint-ci": "stylelint '**/*.scss' --custom-formatter=node_modules/stylelint-checkstyle-formatter > ./ci/results/stylelint-results.xml",
        "stylelint-fix": "stylelint '**/*.scss' --fix",
        "test": "vitest watch",
        "test-ci": "vitest run --reporter=junit --outputFile=./ci/results/test-results.xml",
        "test-once": "vitest run",
        "validate": "tsc && export BROWSERSLIST_IGNORE_OLD_DATA=true && npm run dep-cruiser && npm run eslint && npm run stylelint && npm run validate-locales && npm run prettier-check && npm run validate-theming && npm run validate-ui-components",
        "validate-esm": "node --input-type=module --eval 'import \"@gooddata/sdk-ui-kit\"'",
        "validate-locales": "cd ../sdk-ui && npm run validate-locales",
        "validate-theming": "npm run scss && node scripts/validateCss.js",
        "validate-ui-components": "node scripts/validateUiComponents.js"
    },
    "dependencies": {
        "@aaronhayes/react-use-hubspot-form": "^2.1.2",
        "@codemirror/autocomplete": "^6.19.0",
        "@codemirror/commands": "~6.9.0",
        "@codemirror/language": "~6.11.3",
        "@codemirror/state": "~6.5.2",
        "@codemirror/view": "~6.38.5",
        "@floating-ui/dom": "1.6.13",
        "@floating-ui/react": "~0.27.12",
        "@floating-ui/react-dom": "2.1.2",
        "@gooddata/number-formatter": "^1.0.6",
        "@gooddata/sdk-backend-spi": "workspace:*",
        "@gooddata/sdk-model": "workspace:*",
        "@gooddata/sdk-ui": "workspace:*",
        "@gooddata/sdk-ui-theme-provider": "workspace:*",
        "@gooddata/util": "workspace:*",
        "@lezer/highlight": "~1.2.1",
        "@tanstack/react-virtual": "^3.11.1",
        "@types/mdast": "^4.0.4",
        "@types/unist": "^3.0.3",
        "classnames": "^2.3.1",
        "copy-to-clipboard": "^3.3.3",
        "cron-parser": "^5.1.1",
        "date-fns": "^4.1.0",
        "date-fns-tz": "^3.2.0",
        "debounce-promise": "^3.1.2",
        "default-import": "1.1.5",
        "fixed-data-table-2": "^2.0.22",
        "lodash-es": "^4.17.21",
        "moment": "^2.29.4",
        "react-content-loader": "^7.1.1",
        "react-day-picker": "9.9.0",
        "react-fast-compare": "^3.2.0",
        "react-intl": "7.1.11",
        "react-loading-skeleton": "^3.4.0",
        "react-markdown": "^10.1.0",
        "react-measure": "^2.5.2",
        "react-portal": "^4.3.0",
        "react-responsive": "^10.0.1",
        "react-select": "^5.10.2",
        "react-textarea-autosize": "8.5.6",
        "tinycolor2": "^1.4.2",
        "ts-invariant": "^0.7.5",
        "tslib": "2.8.1",
        "unified": "^11.0.5",
        "uuid": "^11.1.0"
    },
    "devDependencies": {
        "@gooddata/eslint-config": "workspace:*",
        "@gooddata/reference-workspace": "workspace:*",
        "@gooddata/sdk-backend-mockingbird": "workspace:*",
        "@gooddata/stylelint-config": "^5.0.0",
        "@microsoft/api-documenter": "^7.17.0",
        "@microsoft/api-extractor": "^7.52.8",
        "@testing-library/dom": "10.4.0",
        "@testing-library/react": "16.3.0",
        "@testing-library/user-event": "^14.6.1",
        "@types/debounce-promise": "^3.1.4",
        "@types/kefir": "^3.8.5",
        "@types/lodash-es": "^4.17.12",
        "@types/node": "^22.13.0",
        "@types/raf": "^3.4.0",
        "@types/react": "19.1.11",
        "@types/react-dom": "19.1.7",
        "@types/react-measure": "^2.0.12",
        "@types/react-portal": "^4.0.7",
        "@types/tinycolor2": "^1.4.2",
        "@typescript-eslint/eslint-plugin": "8.46.0",
        "@typescript-eslint/parser": "8.46.0",
        "@vitest/eslint-plugin": "1.3.5",
        "dependency-cruiser": "^13.0.3",
        "eslint": "^8.57.1",
        "eslint-config-prettier": "10.1.8",
        "eslint-plugin-eslint-comments": "3.2.0",
        "eslint-plugin-header": "3.1.1",
        "eslint-plugin-import": "2.32.0",
        "eslint-plugin-import-esm": "1.2.1",
        "eslint-plugin-no-only-tests": "2.6.0",
        "eslint-plugin-prettier": "5.5.4",
        "eslint-plugin-react": "7.37.5",
        "eslint-plugin-react-hooks": "4.6.0",
        "eslint-plugin-regexp": "1.15.0",
        "eslint-plugin-sonarjs": "0.16.0",
        "eslint-plugin-tsdoc": "0.2.14",
        "happy-dom": "18.0.1",
        "kefir": "^3.8.8",
        "prettier": "^3.6.2",
        "raf": "^3.4.1",
        "react": "19.1.1",
        "react-dom": "19.1.1",
        "sass": "1.66.1",
        "stylelint": "^13.8.0",
        "stylelint-checkstyle-formatter": "^0.1.2",
        "stylelint-config-prettier": "^9.0.5",
        "svgo": "^2.8.0",
        "typescript": "5.8.3",
        "vitest": "3.2.4",
        "vitest-dom": "0.1.1"
    },
    "peerDependencies": {
        "react": "^18.0.0 || ^19.0.0",
        "react-dom": "^18.0.0 || ^19.0.0"
    }
}
