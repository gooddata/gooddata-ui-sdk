{
    "name": "@gooddata/sdk-ui-dashboard",
    "version": "11.0.0-alpha.2",
    "description": "GoodData SDK - Dashboard Component",
    "repository": {
        "type": "git",
        "url": "https://github.com/gooddata/gooddata-ui-sdk.git",
        "directory": "libs/sdk-ui-dashboard"
    },
    "license": "LicenseRef-LICENSE",
    "author": "GoodData Corporation",
    "sideEffects": [
        "*.css",
        "*.svg"
    ],
    "type": "module",
    "exports": {
        ".": "./esm/index.js",
        "./internal": "./esm/internal.js",
        "./styles/css/*": "./styles/css/*",
        "./styles/scss/*": "./styles/scss/*",
        "./esm/assets/*": "./esm/assets/*"
    },
    "browser": "./esm/index.js",
    "types": "./esm/index.d.ts",
    "files": [
        "esm/**/*.js",
        "esm/**/*.json",
        "esm/**/*.d.ts",
        "esm/**/*.map",
        "esm/**/*.svg",
        "esm/**/*.gif",
        "styles",
        "NOTICE"
    ],
    "scripts": {
        "_phase:build": "npm run build",
        "_phase:post-build": "npm run build-dynamic-files",
        "_phase:test": "npm run build-dynamic-files && npm run test-once",
        "_phase:validate": "npm run build-dynamic-files && npm run validate",
        "api-extractor": "mkdir -p api && [ -z \"${CI}\" ] && (api-extractor run -l; exit 0) || (api-extractor run)",
        "build": "bash scripts/build.sh",
        "build-dynamic-files": "bash scripts/build.sh --genFilesOnly",
        "clean": "../../common/scripts/clean-command-state.sh && rm -rf ci dist esm coverage *.log styles/css tsconfig.tsbuildinfo",
        "dep-cruiser": "depcruise --validate .dependency-cruiser.cjs --output-type err-long src/",
        "dep-cruiser-ci": "depcruise --validate .dependency-cruiser.cjs --output-type err-long src/",
        "eslint": "eslint -c .eslintrc.cjs src/",
        "eslint-ci": "mkdir -p ./ci/results && eslint -f checkstyle -o ci/results/eslint-results.xml -c .eslintrc.cjs src/",
        "eslint-fix": "eslint -c .eslintrc.cjs src/ --fix",
        "prepack": "cp -f ../../NOTICE ./NOTICE",
        "prepublishOnly": "npm run build",
        "prettier-check": "prettier --check '{src,stories,styles,__mocks__}/**/*.{ts,tsx,json,scss,md,yaml,html}'",
        "prettier-write": "prettier --write '{src,stories,styles,__mocks__}/**/*.{ts,tsx,json,scss,md,yaml,html}'",
        "scss": "sass --load-path=node_modules styles/scss:styles/css",
        "stylelint": "stylelint '**/*.scss'",
        "stylelint-ci": "stylelint '**/*.scss' --custom-formatter=node_modules/stylelint-checkstyle-formatter > ./ci/results/stylelint-results.xml",
        "stylelint-fix": "stylelint '**/*.scss' --fix",
        "test": "vitest --watch",
        "test-ci": "vitest run --reporter=junit --outputFile=./ci/results/test-results.xml",
        "test-once": "vitest run",
        "test-once-with-coverage": "vitest --coverage=true",
        "validate": "tsc && export BROWSERSLIST_IGNORE_OLD_DATA=true && npm run dep-cruiser && npm run eslint && npm run stylelint && npm run validate-locales && npm run prettier-check && npm run validate-theming",
        "validate-esm": "node --input-type=module --eval 'import \"@gooddata/sdk-ui-dashboard\"'",
        "validate-locales": "i18n-toolkit",
        "validate-locales-ci": "i18n-toolkit",
        "validate-theming": "npm run scss && node scripts/validateCss.js"
    },
    "dependencies": {
        "@codemirror/language": "~6.11.0",
        "@evil-internetmann/react-dnd": "16.2.1",
        "@gooddata/number-formatter": "^1.0.6",
        "@gooddata/sdk-backend-base": "workspace:*",
        "@gooddata/sdk-backend-spi": "workspace:*",
        "@gooddata/sdk-model": "workspace:*",
        "@gooddata/sdk-ui": "workspace:*",
        "@gooddata/sdk-ui-ext": "workspace:*",
        "@gooddata/sdk-ui-filters": "workspace:*",
        "@gooddata/sdk-ui-geo": "workspace:*",
        "@gooddata/sdk-ui-kit": "workspace:*",
        "@gooddata/sdk-ui-pivot": "workspace:*",
        "@gooddata/sdk-ui-theme-provider": "workspace:*",
        "@gooddata/sdk-ui-vis-commons": "workspace:*",
        "@gooddata/util": "workspace:*",
        "@lezer/highlight": "~1.2.1",
        "@react-aria/interactions": "^3",
        "@reduxjs/toolkit": "2.9.0",
        "classnames": "^2.3.1",
        "date-fns": "^4.1.0",
        "default-import": "1.1.5",
        "immer": "10.1.1",
        "json-stable-stringify": "1.3.0",
        "lodash-es": "^4.17.21",
        "lru-cache": "11.1.0",
        "react-dnd-html5-backend": "^14.1.0",
        "react-intl": "7.1.11",
        "react-lines-ellipsis": "^0.16.1",
        "react-loading-skeleton": "^3.4.0",
        "react-measure": "^2.5.2",
        "react-redux": "9.2.0",
        "react-select": "^5.10.2",
        "redux": "5.0.1",
        "redux-batched-actions": "^0.5.0",
        "redux-saga": "^1.3.0",
        "ts-invariant": "^0.7.5",
        "tslib": "2.8.1",
        "uuid": "^11.1.0"
    },
    "devDependencies": {
        "@gooddata/eslint-config": "^4.1.1",
        "@gooddata/i18n-toolkit": "workspace:*",
        "@gooddata/reference-workspace": "workspace:*",
        "@gooddata/sdk-backend-mockingbird": "workspace:*",
        "@microsoft/api-documenter": "^7.17.0",
        "@microsoft/api-extractor": "^7.52.8",
        "@testing-library/dom": "10.4.0",
        "@testing-library/react": "16.3.0",
        "@testing-library/user-event": "^14.6.1",
        "@types/lodash-es": "^4.17.12",
        "@types/node": "^22.13.0",
        "@types/raf": "^3.4.0",
        "@types/react": "19.1.11",
        "@types/react-dom": "19.1.7",
        "@types/react-lines-ellipsis": "^0.15.6",
        "@types/react-measure": "^2.0.12",
        "@types/react-redux": "7.1.34",
        "@typescript-eslint/eslint-plugin": "^8.38.0",
        "@typescript-eslint/parser": "^8.38.0",
        "concurrently": "^6.0.2",
        "dependency-cruiser": "^13.0.3",
        "eslint": "^8.57.1",
        "eslint-config-prettier": "^10.1.5",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-header": "^3.0.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-import-esm": "^1.2.1",
        "eslint-plugin-jest": "^29.0.1",
        "eslint-plugin-no-only-tests": "^2.4.0",
        "eslint-plugin-prettier": "^5.5.1",
        "eslint-plugin-react": "^7.31.10",
        "eslint-plugin-react-hooks": "^4.5.0",
        "eslint-plugin-regexp": "^1.9.0",
        "eslint-plugin-sonarjs": "^0.16.0",
        "eslint-plugin-tsdoc": "^0.2.14",
        "happy-dom": "18.0.1",
        "prettier": "^3.6.2",
        "raf": "^3.4.1",
        "react": "19.1.1",
        "react-dom": "19.1.1",
        "sass": "1.66.1",
        "stylelint": "^13.8.0",
        "stylelint-checkstyle-formatter": "^0.1.2",
        "stylelint-config-prettier": "^9.0.5",
        "svgo": "^2.8.0",
        "typescript": "5.8.3",
        "vitest": "3.2.4",
        "vitest-dom": "0.1.1"
    },
    "peerDependencies": {
        "react": "^18.0.0 || ^19.0.0",
        "react-dom": "^18.0.0 || ^19.0.0"
    }
}
