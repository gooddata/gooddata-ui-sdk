{
    "name": "@gooddata/sdk-ui-tests-e2e",
    "version": "1.0.0",
    "description": "GoodData SDK TypeScript & React skeleton",
    "repository": {
        "type": "git",
        "url": "https://github.com/gooddata/gooddata-ui-sdk.git",
        "directory": "libs/sdk-ui-tests-e2e"
    },
    "author": "GoodData Corporation",
    "license": "LicenseRef-LICENSE",
    "private": true,
    "main": "dist/index.js",
    "module": "esm/index.js",
    "browser": "dist/index.js",
    "typings": "esm/index.d.ts",
    "sideEffects": [
        "*.css",
        "*.svg"
    ],
    "files": [
        "dist/**/*.js",
        "dist/**/*.json",
        "dist/**/*.d.ts",
        "dist/**/*.map",
        "dist/**/*.svg",
        "esm/**/*.js",
        "esm/**/*.json",
        "esm/**/*.d.ts",
        "esm/**/*.map",
        "esm/**/*.svg",
        "styles"
    ],
    "config": {
        "eslint": "-c .eslintrc.cjs --ext js,jsx,ts,tsx ./"
    },
    "scripts": {
        "clean": "rm -rf ci dist esm scenarios/build coverage *.log styles/css",
        "api-extractor": "mkdir -p api && [ -z \"${CI}\" ] && (api-extractor run -l) || (api-extractor run)",
        "build": "tsc -p tsconfig.build.json",
        "dev": "bash scripts/build.sh --dev-watch",
        "eslint": "eslint -c .eslintrc.cjs --ext js,jsx,ts,tsx ./",
        "eslint-ci": "mkdir -p ./ci/results && eslint -f checkstyle -o ci/results/eslint-results.xml .eslintrc.cjs --ext js,jsx,ts,tsx ./",
        "prettier-check": "prettier --check './**/*.{ts,js,cjs,tsx,jsx,json,scss,md,yaml,html}'",
        "prettier-write": "prettier --write './**/*.{ts,js,cjs,tsx,jsx,json,scss,md,yaml,html}'",
        "dep-cruiser": "depcruise --validate .dependency-cruiser.cjs --output-type err-long cypress/",
        "dep-cruiser-ci": "depcruise --validate .dependency-cruiser.cjs --output-type err-long cypress/",
        "validate": "npm run dep-cruiser && npm run eslint && npm run prettier-check",
        "validate-ci": "npm run dep-cruiser-ci && npm run eslint-ci && npm run prettier-check",
        "docker-build": "docker build --file Dockerfile -t gooddata-ui-sdk .",
        "docker-build-local": "docker build --file Dockerfile_local -t gooddata-ui-sdk .",
        "run-integrated": "node ./scripts/run_integrated.js",
        "run-isolated": "docker-compose -f docker-compose-isolated.yaml up --abort-on-container-exit --exit-code-from isolated-tests --force-recreate --always-recreate-deps --renew-anon-volumes",
        "run-isolated-record": "MODE=record npm run run-isolated-local",
        "run-isolated-local": "npm run docker-build-local && IMAGE_ID=gooddata-ui-sdk npm run run-isolated",
        "run-isolated-ci": "npm run prepare-recording-workspace-id && npm run docker-build && IMAGE_ID=gooddata-ui-sdk npm run run-isolated",
        "run-scenarios-local": "node ./scripts/run-scenarios-local.js",
        "local-dev-test-visual": "node ./scripts/run_local_dev_isolated.js",
        "local-dev-test-visual-recordings": "node ./scripts/run_local_dev_isolated.js --with-recordings",
        "local-dev-wiremock": "node ./scripts/run_local_dev_wiremock.js",
        "local-dev-wiremock-proxy": "node ./scripts/run_local_dev_wiremock.js --proxy",
        "local-dev-wiremock-proxy-verbose": "node ./scripts/run_local_dev_wiremock.js --proxy --verbose",
        "prepare-recording-workspace-id": "node ./reference_workspace/prepareRecordingWorkspaceId.js",
        "build-scenarios": "webpack --config scenarios/webpack.config.cjs --config-name default",
        "start-scenarios": "webpack serve --mode development --config scenarios/webpack.config.cjs --config-name local-run",
        "export-fixture": "node ./reference_workspace/export_workspace_fixture.js",
        "create-ref-workspace": "node ./reference_workspace/create_ref_workspace.js && npm run prettier-write",
        "delete-ref-workspace": "node ./reference_workspace/delete_ref_workspace.js",
        "update-ref-workspace": "npm run delete-ref-workspace && npm run create-ref-workspace"
    },
    "dependencies": {
        "@babel/polyfill": "^7.7.2",
        "@gooddata/catalog-export": "workspace:*",
        "@gooddata/eslint-config": "^4.1.0",
        "@gooddata/fixtures": "^1.27.0",
        "@gooddata/sdk-backend-bear": "workspace:*",
        "@gooddata/sdk-backend-spi": "workspace:*",
        "@gooddata/sdk-backend-tiger": "workspace:*",
        "@gooddata/sdk-model": "workspace:*",
        "@gooddata/sdk-ui": "workspace:*",
        "@gooddata/sdk-ui-charts": "workspace:*",
        "@gooddata/sdk-ui-dashboard": "workspace:*",
        "@gooddata/sdk-ui-ext": "workspace:*",
        "@gooddata/sdk-ui-filters": "workspace:*",
        "@gooddata/sdk-ui-geo": "workspace:*",
        "@gooddata/sdk-ui-kit": "workspace:*",
        "@gooddata/sdk-ui-pivot": "workspace:*",
        "@gooddata/util": "workspace:*",
        "axios": "^1.4.0",
        "dotenv": "16.0.3",
        "lodash": "^4.17.19",
        "pdf-parse": "^1.1.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-intl": "^6.4.1",
        "react-router-dom": "^5.2.0",
        "tslib": "^2.5.0",
        "wait-on": "^6.0.0",
        "xml2js": "^0.5.0",
        "node-xlsx": "^0.21.0"
    },
    "type": "module",
    "devDependencies": {
        "@babel/cli": "^7.7.2",
        "@babel/core": "^7.7.2",
        "@babel/plugin-proposal-class-properties": "^7.7.2",
        "@babel/plugin-proposal-object-rest-spread": "^7.7.2",
        "@babel/plugin-transform-async-to-generator": "^7.7.2",
        "@babel/plugin-transform-typescript": "^7.7.2",
        "@babel/preset-env": "^7.7.2",
        "@babel/preset-react": "^7.7.2",
        "@babel/preset-typescript": "^7.7.2",
        "@babel/runtime": "^7.7.2",
        "@cypress/grep": "^3.1.5",
        "@microsoft/api-documenter": "^7.17.0",
        "@microsoft/api-extractor": "^7.36.0",
        "@types/lodash": "^4.14.158",
        "@types/node": "^18.17.6",
        "@types/pdf-parse": "^1.1.1",
        "@types/raf": "^3.4.0",
        "@types/react": "^18.2.0",
        "@types/react-dom": "^18.2.1",
        "@types/react-router-dom": "^5.1.5",
        "@typescript-eslint/eslint-plugin": "^5.57.1",
        "@typescript-eslint/parser": "^5.57.1",
        "babel-loader": "^8.0.5",
        "babel-plugin-lodash": "^3.3.4",
        "circular-dependency-plugin": "^5.2.0",
        "clean-webpack-plugin": "^4.0.0",
        "compression-webpack-plugin": "^7.1.0",
        "css-loader": "^6.7.1",
        "cypress": "^12.10.0",
        "cypress-real-events": "^1.7.6",
        "dependency-cruiser": "^13.0.3",
        "dotenv-webpack": "^7.0.2",
        "eslint": "^8.25.0",
        "eslint-import-resolver-typescript": "^3.5.5",
        "eslint-plugin-header": "^3.0.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-jest": "^27.1.1",
        "eslint-plugin-no-only-tests": "^2.4.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.31.10",
        "eslint-plugin-react-hooks": "^4.5.0",
        "eslint-plugin-regexp": "^1.9.0",
        "eslint-plugin-sonarjs": "^0.16.0",
        "eslint-plugin-tsdoc": "^0.2.14",
        "fork-ts-checker-webpack-plugin": "^7.2.13",
        "html-webpack-plugin": "^5.3.1",
        "npm-run-all": "^4.1.5",
        "prettier": "~2.5.0",
        "prettier-loader": "^3.3.0",
        "raf": "^3.4.1",
        "sass": "^1.52.3",
        "sass-loader": "^10.1.1",
        "source-map-loader": "^4.0.0",
        "style-loader": "^3.3.1",
        "styled-jsx": "^5.1.0",
        "typescript": "5.0.2",
        "webpack": "^5.58.0",
        "webpack-cli": "^4.9.2",
        "webpack-dev-server": "^4.9.1"
    }
}
