{
    "name": "@gooddata/sdk-ui-tests-e2e",
    "version": "1.0.0",
    "description": "GoodData SDK TypeScript & React skeleton",
    "repository": {
        "type": "git",
        "url": "https://github.com/gooddata/gooddata-ui-sdk.git",
        "directory": "libs/sdk-ui-tests-e2e"
    },
    "author": "GoodData Corporation",
    "license": "LicenseRef-LICENSE",
    "private": true,
    "main": "dist/index.js",
    "module": "esm/index.js",
    "browser": "dist/index.js",
    "typings": "esm/index.d.ts",
    "sideEffects": [
        "*.css",
        "*.svg"
    ],
    "files": [
        "dist/**/*.js",
        "dist/**/*.json",
        "dist/**/*.d.ts",
        "dist/**/*.map",
        "dist/**/*.svg",
        "esm/**/*.js",
        "esm/**/*.json",
        "esm/**/*.d.ts",
        "esm/**/*.map",
        "esm/**/*.svg",
        "styles"
    ],
    "config": {
        "eslint": "-c .eslintrc.js --ext ts,tsx cypress/"
    },
    "scripts": {
        "clean": "rm -rf ci dist esm coverage *.log styles/css",
        "api-extractor": "mkdir -p api && [ -z \"${CI}\" ] && (api-extractor run -l) || (api-extractor run)",
        "build": "tsc -p tsconfig.build.json",
        "dev": "bash scripts/build.sh --dev-watch",
        "eslint": "eslint $npm_package_config_eslint",
        "eslint-ci": "mkdir -p ./ci/results && eslint -f checkstyle -o ci/results/eslint-results.xml $npm_package_config_eslint",
        "prettier-check": "prettier --check '{src,stories,styles,__mocks__}/**/*.{ts,tsx,json,scss,md,yaml,html}'",
        "prettier-write": "prettier --write '{src,stories,styles,__mocks__}/**/*.{ts,tsx,json,scss,md,yaml,html}'",
        "dep-cruiser": "depcruise --validate .dependency-cruiser.js --output-type err-long cypress/",
        "dep-cruiser-ci": "depcruise --validate .dependency-cruiser.js --output-type err-long cypress/",
        "validate": "npm run dep-cruiser && npm run eslint && npm run prettier-check",
        "validate-ci": "npm run dep-cruiser-ci && npm run eslint-ci && npm run prettier-check",
        "cypress": "node ./scripts/run_cypress_local.js",
        "run-isolated": "docker-compose -f docker-compose-isolated.yaml up --abort-on-container-exit --exit-code-from isolated-tests --force-recreate --always-recreate-deps --renew-anon-volumes",
        "docker-build-local": "docker build --file Dockerfile_local -t gooddata-ui-sdk .",
        "run-isolated-local": "npm run docker-build-local && IMAGE_ID=gooddata-ui-sdk npm run run-isolated",
        "run-scenarios-local": "node ./scripts/run-scenarios-local.js"
    },
    "dependencies": {
        "axios": "^0.21.1",
        "dotenv": "^8.2.0",
        "lodash": "^4.17.19",
        "react-intl": "^3.6.0",
        "tslib": "^2.0.0",
        "wait-on": "^6.0.0"
    },
    "peerDependencies": {
        "react": "^16.10.0",
        "react-dom": "^16.10.0"
    },
    "devDependencies": {
        "@gooddata/eslint-config": "^2.1.0",
        "@gooddata/sdk-backend-spi": "^8.6.0-alpha.25",
        "@gooddata/util": "^8.6.0-alpha.25",
        "@microsoft/api-documenter": "^7.12.13",
        "@microsoft/api-extractor": "^7.18.4",
        "@types/enzyme-adapter-react-16": "^1.0.5",
        "@types/enzyme": "^3.10.3",
        "@types/lodash": "^4.14.158",
        "@types/raf": "^3.4.0",
        "@types/react-dom": "^16.9.3",
        "@types/react": "^16.9.11",
        "@typescript-eslint/eslint-plugin": "^4.14.2",
        "@typescript-eslint/parser": "^4.14.2",
        "cypress": "~7.7.0",
        "dependency-cruiser": "^9.9.5",
        "enzyme": "^3.10.0",
        "enzyme-adapter-react-16": "^1.15.1",
        "eslint": "^7.5.0",
        "eslint-plugin-header": "^3.0.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-no-only-tests": "^2.4.0",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-react-hooks": "^4.0.8",
        "eslint-plugin-react": "^7.20.5",
        "eslint-plugin-sonarjs": "^0.9.1",
        "node-sass": "^4.13.0",
        "node-sass-magic-importer": "^5.3.2",
        "prettier": "~2.3.0",
        "raf": "^3.4.1",
        "react": "^16.10.0",
        "react-dom": "^16.10.0",
        "typescript": "4.0.2"
    }
}
