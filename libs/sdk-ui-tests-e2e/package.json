{
    "name": "@gooddata/sdk-ui-tests-e2e",
    "version": "1.0.0",
    "description": "GoodData SDK TypeScript & React skeleton",
    "repository": {
        "type": "git",
        "url": "https://github.com/gooddata/gooddata-ui-sdk.git",
        "directory": "libs/sdk-ui-tests-e2e"
    },
    "author": "GoodData Corporation",
    "license": "LicenseRef-LICENSE",
    "private": true,
    "main": "dist/index.js",
    "module": "esm/index.js",
    "browser": "dist/index.js",
    "typings": "esm/index.d.ts",
    "sideEffects": [
        "*.css",
        "*.svg"
    ],
    "files": [
        "dist/**/*.js",
        "dist/**/*.json",
        "dist/**/*.d.ts",
        "dist/**/*.map",
        "dist/**/*.svg",
        "esm/**/*.js",
        "esm/**/*.json",
        "esm/**/*.d.ts",
        "esm/**/*.map",
        "esm/**/*.svg",
        "styles"
    ],
    "config": {
        "eslint": "-c .eslintrc.js --ext ts,tsx cypress/"
    },
    "scripts": {
        "clean": "rm -rf ci dist esm coverage *.log styles/css",
        "api-extractor": "mkdir -p api && [ -z \"${CI}\" ] && (api-extractor run -l) || (api-extractor run)",
        "build": "tsc -p tsconfig.build.json",
        "dev": "bash scripts/build.sh --dev-watch",
        "eslint": "eslint $npm_package_config_eslint",
        "eslint-ci": "mkdir -p ./ci/results && eslint -f checkstyle -o ci/results/eslint-results.xml $npm_package_config_eslint",
        "prettier-check": "prettier --check '{src,stories,styles,__mocks__}/**/*.{ts,tsx,json,scss,md,yaml,html}'",
        "prettier-write": "prettier --write '{src,stories,styles,__mocks__}/**/*.{ts,tsx,json,scss,md,yaml,html}'",
        "dep-cruiser": "depcruise --validate .dependency-cruiser.js --output-type err-long cypress/",
        "dep-cruiser-ci": "depcruise --validate .dependency-cruiser.js --output-type err-long cypress/",
        "validate": "npm run dep-cruiser && npm run eslint && npm run prettier-check",
        "validate-ci": "npm run dep-cruiser-ci && npm run eslint-ci && npm run prettier-check",
        "cypress": "node ./scripts/run_cypress_local.js",
        "docker-build": "npm run build-scenarios && DOCKER_BUILDKIT=0 docker build --file Dockerfile -t gooddata-ui-sdk .",
        "docker-build-local": "docker build --file Dockerfile_local -t gooddata-ui-sdk .",
        "run-isolated": "docker-compose -f docker-compose-isolated.yaml up --abort-on-container-exit --exit-code-from isolated-tests --force-recreate --always-recreate-deps --renew-anon-volumes",
        "run-isolated-record": "MODE=record npm run run-isolated-local",
        "run-isolated-local": "npm run docker-build-local && IMAGE_ID=gooddata-ui-sdk npm run run-isolated",
        "run-isolated-ci": "npm run docker-build && IMAGE_ID=gooddata-ui-sdk npm run run-isolated",
        "run-scenarios-local": "node ./scripts/run-scenarios-local.js",
        "build-scenarios": "webpack --config scenarios/webpack.config.js",
        "refresh-md": "./scripts/refresh-md.sh"
    },
    "dependencies": {
        "@gooddata/catalog-export": "^8.11.0-beta.1",
        "@gooddata/eslint-config": "^2.1.0",
        "@gooddata/sdk-backend-bear": "^8.11.0-beta.1",
        "@gooddata/sdk-backend-spi": "^8.11.0-beta.1",
        "@gooddata/sdk-model": "^8.11.0-beta.1",
        "@gooddata/sdk-ui": "^8.11.0-beta.1",
        "@gooddata/sdk-ui-charts": "^8.11.0-beta.1",
        "@gooddata/sdk-ui-dashboard": "^8.11.0-beta.1",
        "@gooddata/sdk-ui-ext": "^8.11.0-beta.1",
        "@gooddata/sdk-ui-filters": "^8.11.0-beta.1",
        "@gooddata/sdk-ui-geo": "^8.11.0-beta.1",
        "@gooddata/sdk-ui-kit": "^8.11.0-beta.1",
        "@gooddata/sdk-ui-pivot": "^8.11.0-beta.1",
        "@gooddata/util": "^8.11.0-beta.1",
        "axios": "^0.21.4",
        "babel-polyfill": "^6.26.0",
        "babel-runtime": "^6.26.0",
        "dotenv": "^10.0.0",
        "lodash": "^4.17.19",
        "react": "^17.0.2",
        "react-app-polyfill": "~2.0.0",
        "react-dom": "^17.0.2",
        "react-intl": "^5.23.0",
        "react-router-dom": "^5.2.0",
        "tslib": "^2.0.0",
        "wait-on": "^6.0.0",
        "xml2js": "^0.4.23"
    },
    "devDependencies": {
        "@babel/cli": "^7.7.2",
        "@babel/core": "^7.7.2",
        "@babel/plugin-proposal-class-properties": "^7.7.2",
        "@babel/plugin-proposal-object-rest-spread": "^7.7.2",
        "@babel/plugin-transform-async-to-generator": "^7.7.2",
        "@babel/plugin-transform-typescript": "^7.7.2",
        "@babel/polyfill": "^7.7.2",
        "@babel/preset-env": "^7.7.2",
        "@babel/preset-react": "^7.7.2",
        "@babel/preset-typescript": "^7.7.2",
        "@babel/runtime": "^7.7.2",
        "@microsoft/api-documenter": "^7.17.0",
        "@microsoft/api-extractor": "^7.20.0",
        "@types/enzyme": "^3.10.3",
        "@types/lodash": "^4.14.158",
        "@types/node": "^16.11.11",
        "@types/raf": "^3.4.0",
        "@types/react": "^17.0.34",
        "@types/react-dom": "^17.0.11",
        "@typescript-eslint/eslint-plugin": "^5.5.0",
        "@typescript-eslint/parser": "^5.5.0",
        "@wojtekmaj/enzyme-adapter-react-17": "^0.6.5",
        "babel-loader": "^8.0.5",
        "babel-plugin-lodash": "^3.3.4",
        "circular-dependency-plugin": "^5.2.0",
        "clean-webpack-plugin": "^4.0.0",
        "compression-webpack-plugin": "^7.1.0",
        "css-loader": "^6.7.1",
        "cypress": "~7.7.0",
        "cypress-real-events": "^1.7.0",
        "dependency-cruiser": "^10.1.1",
        "dotenv-webpack": "^7.0.2",
        "enzyme": "^3.10.0",
        "eslint": "^8.3.0",
        "eslint-plugin-header": "^3.0.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-jest": "^25.3.0",
        "eslint-plugin-no-only-tests": "^2.4.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.20.5",
        "eslint-plugin-react-hooks": "^4.5.0",
        "eslint-plugin-regexp": "^1.7.0",
        "eslint-plugin-sonarjs": "^0.14.0",
        "eslint-plugin-tsdoc": "^0.2.14",
        "fork-ts-checker-webpack-plugin": "^7.2.13",
        "html-webpack-plugin": "^5.3.1",
        "jest": "^27.5.1",
        "jest-junit": "^3.0.0",
        "npm-run-all": "^4.1.5",
        "prettier": "~2.5.0",
        "prettier-loader": "^3.3.0",
        "raf": "^3.4.1",
        "sass": "^1.52.3",
        "sass-loader": "^10.1.1",
        "source-map-loader": "^4.0.0",
        "speed-measure-webpack-plugin": "^1.5.0",
        "style-loader": "^3.3.1",
        "styled-jsx": "^4.0.1",
        "typescript": "4.0.2",
        "webpack": "^5.58.0",
        "webpack-cli": "^4.9.2",
        "webpack-dev-server": "^4.9.1"
    }
}
