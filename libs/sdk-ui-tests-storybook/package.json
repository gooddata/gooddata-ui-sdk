{
    "name": "@gooddata/sdk-ui-tests-storybook",
    "version": "8.2.0-alpha.7",
    "description": "GoodData React components storybook",
    "license": "LicenseRef-LICENSE",
    "author": "GoodData Corporation",
    "repository": {
        "type": "git",
        "url": "https://github.com/gooddata/gooddata-ui-sdk.git",
        "directory": "libs/sdk-ui-tests-storybook"
    },
    "files": [
        "neobackstop",
        "stories",
        ".storybook"
    ],
    "type": "module",
    "scripts": {
        "_phase:build": "npm run build",
        "_phase:test": "npm run test-once",
        "_phase:validate": "npm run validate",
        "build": "npm-run-all -p build-check build-storybook",
        "build-check": "tsgo",
        "build-storybook": "export NODE_OPTIONS=\"--max_old_space_size=8192\" && storybook build -c .storybook -o dist --quiet",
        "clean": "../../common/scripts/clean-command-state.sh && rm -rf dist coverage *.log tsconfig.tsbuildinfo",
        "neobackstop-approve-docker": "npm run neobackstop-prepare-docker && bash neobackstop/run-neobackstop-compose.sh approve",
        "neobackstop-approve-local": "npm run neobackstop-prepare-local && bash neobackstop/run-neobackstop-compose.sh approve",
        "neobackstop-prepare-docker": "npm run scenario-extractor-docker",
        "neobackstop-prepare-local": "rm -rf neobackstop/debug/dist && cp -r dist neobackstop/debug && npm run scenario-extractor-local",
        "neobackstop-test-docker": "npm run neobackstop-prepare-docker && bash neobackstop/run-neobackstop-compose.sh test",
        "neobackstop-test-local": "npm run neobackstop-prepare-local && bash neobackstop/run-neobackstop-compose.sh test",
        "format-check": "oxfmt --check .",
        "format-write": "oxfmt .",
        "scenario-extractor-docker": "DOCKER=true vite-node scripts/scenario-extractor.ts",
        "scenario-extractor-local": "vite-node scripts/scenario-extractor.ts",
        "story-generator": "vite-node scripts/story-generator.ts && npm run lint-fix",
        "storybook": "storybook dev -p 9001 -c .storybook",
        "storybook-serve": "bash stories/serve-storybook.sh",
        "stylelint": "stylelint '**/*.scss'",
        "stylelint-fix": "stylelint '**/*.scss' --fix",
        "test-once": "npm run scenario-extractor-local",
        "validate": "npm run lint && npm run stylelint && npm run format-check",
        "lint": "oxlint . --type-aware --quiet && eslint .",
        "lint-fix": "oxlint . --type-aware --quiet --fix && eslint . --fix"
    },
    "devDependencies": {
        "@gooddata/eslint-config": "workspace:*",
        "@gooddata/mock-handling": "workspace:*",
        "@gooddata/oxlint-config": "workspace:*",
        "@gooddata/reference-workspace": "workspace:*",
        "@gooddata/sdk-backend-base": "workspace:*",
        "@gooddata/sdk-backend-mockingbird": "workspace:*",
