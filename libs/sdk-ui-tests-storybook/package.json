        "@gooddata/sdk-ui-filters": "workspace:*",
        "@gooddata/sdk-ui-gen-ai": "workspace:*",
        "@gooddata/sdk-ui-geo": "workspace:*",
        "@gooddata/sdk-ui-kit": "workspace:*",
        "@gooddata/sdk-ui-pivot": "workspace:*",
        "@gooddata/sdk-ui-semantic-search": "workspace:*",
        "@gooddata/sdk-ui-tests-scenarios": "workspace:*",
        "@gooddata/sdk-ui-theme-provider": "workspace:*",
        "@gooddata/sdk-ui-web-components": "workspace:*",
        "@gooddata/stylelint-config": "workspace:*",
        "@storybook/addon-a11y": "^10.1.11",
        "@storybook/builder-vite": "^10.1.11",
        "@storybook/cli": "^10.1.11",
        "@storybook/react-vite": "^10.1.11",
        "@testing-library/dom": "10.4.1",
        "@testing-library/react": "16.3.0",
        "@types/lodash-es": "^4.17.12",
        "@types/node": "22.13.0",
        "@types/react": "19.1.11",
        "@types/react-dom": "19.1.7",
        "@types/spark-md5": "^3.0.1",
        "@typescript-eslint/eslint-plugin": "8.52.0",
        "@typescript-eslint/parser": "8.52.0",
        "@typescript/native-preview": "7.0.0-dev.20260202.1",
        "@vitest/eslint-plugin": "1.6.6",
        "eslint": "^9.39.2",
        "eslint-import-resolver-typescript": "4.4.4",
        "eslint-plugin-headers": "1.3.3",
        "eslint-plugin-import-esm": "1.2.1",
        "eslint-plugin-import-x": "4.16.1",
        "eslint-plugin-jsdoc": "62.1.0",
        "eslint-plugin-no-barrel-files": "1.2.2",
        "eslint-plugin-no-only-tests": "3.3.0",
        "eslint-plugin-react": "7.37.5",
        "eslint-plugin-react-hooks": "5.2.0",
        "eslint-plugin-sonarjs": "3.0.6",
        "eslint-plugin-storybook": "10.1.11",
        "fast-glob": "3.3.3",
