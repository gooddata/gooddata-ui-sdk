        "oxlint": "^1.43.0",
        "oxlint-tsgolint": "0.11.4",
        "react": "19.1.1",
        "react-dom": "19.1.1",
        "react-intl": "7.1.11",
        "react-responsive": "^10.0.1",
        "require-from-string": "^2.0.2",
        "rolldown": "1.0.0-rc.3",
        "spark-md5": "^3.0.0",
        "storybook": "^10.1.11",
        "stylelint": "^16.26.1",
        "ts-invariant": "0.10.3",
        "tslib": "2.8.1",
        "typescript": "5.9.3",
        "uuid": "11.1.0",
        "vite": "8.0.0-beta.13",
        "vite-node": "^5.2.0",
        "vite-plugin-static-copy": "3.1.2"
    },
    "peerDependencies": {
        "@gooddata/sdk-ui": "workspace:*",
        "@gooddata/sdk-ui-charts": "workspace:*",
        "@gooddata/sdk-ui-ext": "workspace:*",
        "@gooddata/sdk-ui-filters": "workspace:*",
        "@gooddata/sdk-ui-gen-ai": "workspace:*",
        "@gooddata/sdk-ui-geo": "workspace:*",
