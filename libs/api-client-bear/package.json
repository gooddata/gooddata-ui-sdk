{
    "name": "@gooddata/api-client-bear",
    "version": "0.0.0-dev.0",
    "author": "GoodData",
    "description": "API Client for the GoodData platform",
    "repository": {
        "type": "git",
        "url": "https://github.com/gooddata/gooddata-ui-sdk.git",
        "directory": "libs/api-client-bear"
    },
    "license": "MIT",
    "type": "module",
    "browser": "./esm/gooddata-browser.js",
    "exports": {
        "node": "./esm/gooddata-node.js",
        "default": "./esm/gooddata-browser.js"
    },
    "types": "./esm/gooddata-browser.d.ts",
    "sideEffects": false,
    "files": [
        "esm/**/*.js",
        "esm/**/*.json",
        "esm/**/*.d.ts",
        "esm/**/*.map"
    ],
    "scripts": {
        "clean": "rm -rf ci dist esm coverage *.log",
        "dev": "bash scripts/build.sh --dev-watch",
        "build": "bash scripts/build.sh",
        "build-esm": "tsc -p tsconfig.build.json",
        "build-dev": "bash scripts/build.sh --dev",
        "test": "vitest watch",
        "test-once": "vitest run",
        "test-ci": "vitest run --reporter=junit --outputFile=./ci/results/test-results.xml",
        "eslint": "eslint -c .eslintrc.cjs src/",
        "eslint-fix": "eslint -c .eslintrc.cjs src/ --fix",
        "eslint-ci": "mkdir -p ./ci/results && eslint -f checkstyle -o ci/results/eslint-results.xml -c .eslintrc.cjs src/",
        "prettier-check": "prettier --check '{src,test}/**/*.{ts,tsx,json,scss,md,yaml,html}'",
        "prettier-write": "prettier --write '{src,test}/**/*.{ts,tsx,json,scss,md,yaml,html}'",
        "dep-cruiser": "depcruise --validate .dependency-cruiser.cjs --output-type err-long src/",
        "dep-cruiser-ci": "depcruise --validate .dependency-cruiser.cjs --output-type err-long src/",
        "validate": "npm run dep-cruiser && npm run eslint && npm run prettier-check",
        "validate-ci": "npm run dep-cruiser-ci && npm run eslint-ci && npm run prettier-check"
    },
    "dependencies": {
        "@gooddata/api-model-bear": "workspace:*",
        "fetch-cookie": "^2.1.0",
        "isomorphic-fetch": "^3.0.0",
        "lodash": "^4.17.19",
        "node-fetch": "^3.3.1",
        "qs": "^6.8.0",
        "spark-md5": "^3.0.0",
        "ts-invariant": "^0.7.3",
        "tslib": "^2.5.0"
    },
    "devDependencies": {
        "@gooddata/eslint-config": "^4.1.0",
        "@types/fast-levenshtein": "0.0.1",
        "@types/isomorphic-fetch": "^0.0.34",
        "@types/lodash": "^4.14.158",
        "@types/node": "^18.17.6",
        "@types/node-fetch": "^2.6.4",
        "@types/qs": "^6.5.1",
        "@types/spark-md5": "^3.0.1",
        "@types/tough-cookie": "^4.0.1",
        "@typescript-eslint/eslint-plugin": "^5.57.1",
        "@typescript-eslint/parser": "^5.57.1",
        "async": "^2.6.4",
        "blob-polyfill": "^7.0.20220408",
        "concurrently": "^6.0.2",
        "dependency-cruiser": "^13.0.3",
        "eslint": "^8.25.0",
        "eslint-plugin-header": "^3.0.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-import-esm": "^1.2.1",
        "eslint-plugin-jest": "^27.1.1",
        "eslint-plugin-no-only-tests": "^2.4.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-regexp": "^1.9.0",
        "eslint-plugin-sonarjs": "^0.16.0",
        "eslint-plugin-tsdoc": "^0.2.14",
        "fast-levenshtein": "^2.0.6",
        "fetch-mock": "^9.11.0",
        "jsdom": "^22.1.0",
        "js-object-pretty-print": "^0.2.0",
        "prettier": "~2.5.0",
        "typescript": "5.0.2",
        "vitest": "0.31.4"
    }
}
