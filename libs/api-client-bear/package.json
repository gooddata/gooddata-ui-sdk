{
    "name": "@gooddata/api-client-bear",
    "version": "8.8.0-alpha.0",
    "author": "GoodData",
    "description": "API Client for the GoodData platform",
    "repository": {
        "type": "git",
        "url": "https://github.com/gooddata/gooddata-ui-sdk.git",
        "directory": "libs/api-client-bear"
    },
    "license": "MIT",
    "main": "dist/gooddata-node.js",
    "module": "esm/gooddata-browser.js",
    "browser": "dist/gooddata-browser.js",
    "typings": "esm/gooddata-browser.d.ts",
    "sideEffects": false,
    "files": [
        "dist/**/*.js",
        "dist/**/*.json",
        "dist/**/*.d.ts",
        "dist/**/*.map",
        "esm/**/*.js",
        "esm/**/*.json",
        "esm/**/*.d.ts",
        "esm/**/*.map",
        "umd"
    ],
    "config": {
        "eslint": "-c .eslintrc.js --ext ts src/"
    },
    "scripts": {
        "clean": "rm -rf ci dist esm umd coverage *.log && jest --clearCache",
        "dev": "bash scripts/build.sh --dev-watch",
        "build": "bash scripts/build.sh",
        "build-dev": "bash scripts/build.sh --dev",
        "build-cjs": "tsc -p tsconfig.build.json",
        "build-esm": "tsc -p tsconfig.build.esm.json",
        "build-umd": "rm -rf umd && webpack --mode production",
        "test": "jest --watch",
        "test-once": "jest",
        "test-ci": "JEST_JUNIT_OUTPUT=./ci/results/test-results.xml jest --ci --config jest.ci.js",
        "eslint": "eslint $npm_package_config_eslint",
        "eslint-ci": "mkdir -p ./ci/results && eslint -f checkstyle -o ci/results/eslint-results.xml $npm_package_config_eslint",
        "prettier-check": "prettier --check '{src,test}/**/*.{ts,tsx,json,scss,md,yaml,html}'",
        "prettier-write": "prettier --write '{src,test}/**/*.{ts,tsx,json,scss,md,yaml,html}'",
        "dep-cruiser": "depcruise --validate .dependency-cruiser.js --output-type err-long src/",
        "dep-cruiser-ci": "depcruise --validate .dependency-cruiser.js --output-type err-long src/",
        "validate": "npm run dep-cruiser && npm run eslint && npm run prettier-check",
        "validate-ci": "npm run dep-cruiser-ci && npm run eslint-ci && npm run prettier-check"
    },
    "dependencies": {
        "@gooddata/api-model-bear": "^8.8.0-alpha.0",
        "fetch-cookie": "^0.7.0",
        "isomorphic-fetch": "^3.0.0",
        "lodash": "^4.17.19",
        "node-fetch": "^2.6.1",
        "qs": "^6.8.0",
        "spark-md5": "^3.0.0",
        "ts-invariant": "^0.7.3",
        "tslib": "^2.0.0"
    },
    "devDependencies": {
        "@gooddata/eslint-config": "^2.1.0",
        "@types/fast-levenshtein": "0.0.1",
        "@types/isomorphic-fetch": "^0.0.34",
        "@types/jest": "^26.0.12",
        "@types/lodash": "^4.14.158",
        "@types/node-fetch": "^1.6.7",
        "@types/qs": "^6.5.1",
        "@types/spark-md5": "^3.0.1",
        "@typescript-eslint/eslint-plugin": "^4.14.2",
        "@typescript-eslint/parser": "^4.14.2",
        "async": "^2.1.5",
        "concurrently": "^6.0.2",
        "dependency-cruiser": "^9.9.5",
        "eslint": "^7.5.0",
        "eslint-plugin-header": "^3.0.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-jest": "^24.1.3",
        "eslint-plugin-no-only-tests": "^2.4.0",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-sonarjs": "^0.9.1",
        "fast-levenshtein": "^2.0.6",
        "fetch-mock": "^9.11.0",
        "jest": "^26.4.2",
        "jest-junit": "^3.0.0",
        "js-object-pretty-print": "^0.2.0",
        "lodash-webpack-plugin": "^0.11.0",
        "prettier": "~2.3.0",
        "ts-jest": "^26.3.0",
        "ts-loader": "^8.3.0",
        "typescript": "4.0.2",
        "webpack": "^5.58.0",
        "webpack-cli": "^4.6.0"
    }
}
