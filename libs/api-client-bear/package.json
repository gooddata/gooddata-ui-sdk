{
    "name": "@gooddata/api-client-bear",
    "version": "8.0.0-beta.73",
    "author": "GoodData",
    "description": "API Client for the GoodData platform",
    "license": "LicenseRef-LICENSE",
    "main": "dist/gooddata-node.js",
    "browser": "dist/gooddata-browser.js",
    "typings": "dist/gooddata-browser.d.ts",
    "files": [
        "dist",
        "umd"
    ],
    "config": {
        "eslint": "-c .eslintrc.js --ext ts src/"
    },
    "scripts": {
        "clean": "rm -rf ci umd coverage *.log && jest --clearCache",
        "dev": "tsc -p tsconfig.dev.json --watch",
        "build": "npm run build:dist && npm run build:umd",
        "build:dist": "rm -rf dist && tsc -p tsconfig.build.json",
        "build:umd": "rm -rf umd && webpack -p",
        "prepublishOnly": "npm run build",
        "test": "jest --watch",
        "test-once": "jest",
        "test-ci": "JEST_JUNIT_OUTPUT=./ci/results/test-results.xml jest --ci --config jest.ci.js",
        "eslint": "eslint $npm_package_config_eslint",
        "eslint-ci": "mkdir -p ./ci/results && eslint -f checkstyle -o ci/results/eslint-results.xml $npm_package_config_eslint",
        "prettier-check": "prettier --check '{src,test}/**/*.{ts,tsx,json,scss,md,yaml,html}'",
        "prettier-write": "prettier --write '{src,test}/**/*.{ts,tsx,json,scss,md,yaml,html}'",
        "dep-cruiser": "depcruise --validate .dependency-cruiser.js --output-type err-long src/",
        "dep-cruiser-ci": "depcruise --validate .dependency-cruiser.js --output-type err-long src/",
        "validate": "npm run dep-cruiser && npm run eslint && npm run prettier-check",
        "validate-ci": "npm run dep-cruiser-ci && npm run eslint-ci && npm run prettier-check"
    },
    "repository": "https://github.com/gooddata/gooddata-ui-sdk/tree/master/libs/api-client-bear",
    "dependencies": {
        "@gooddata/api-model-bear": "^8.0.0-beta.73",
        "fetch-cookie": "^0.7.0",
        "isomorphic-fetch": "^2.2.1",
        "lodash": "^4.17.19",
        "md5": "^2.2.1",
        "node-fetch": "^2.6.1",
        "qs": "^6.8.0",
        "ts-invariant": "^0.4.4",
        "tslib": "^2.0.0",
        "uuid": "^3.3.3"
    },
    "devDependencies": {
        "@gooddata/eslint-config": "^2.0.0",
        "@types/fast-levenshtein": "0.0.1",
        "@types/fetch-mock": "^5.*",
        "@types/isomorphic-fetch": "^0.0.34",
        "@types/jest": "^26.0.12",
        "@types/lodash": "^4.14.158",
        "@types/md5": "^2.1.32",
        "@types/node-fetch": "^1.6.7",
        "@types/qs": "^6.5.1",
        "@types/uuid": "^3.4.5",
        "@typescript-eslint/eslint-plugin": "^3.7.1",
        "@typescript-eslint/parser": "^3.7.1",
        "async": "^2.1.5",
        "dependency-cruiser": "^9.9.5",
        "eslint": "^7.5.0",
        "eslint-plugin-header": "^3.0.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-jest": "^23.20.0",
        "eslint-plugin-prettier": "^3.1.4",
        "fast-levenshtein": "^2.0.6",
        "fetch-mock": "^5.12.2",
        "jest": "^26.4.2",
        "jest-junit": "^3.0.0",
        "js-object-pretty-print": "^0.2.0",
        "lodash-webpack-plugin": "^0.11.0",
        "prettier": "~2.0.5",
        "ts-jest": "^26.3.0",
        "ts-loader": "^6.2.1",
        "typescript": "3.9.5",
        "webpack": "^4.44.1",
        "webpack-cli": "^3.3.7",
        "webpack-dev-server": "^3.8.0"
    }
}
