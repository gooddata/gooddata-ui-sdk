{
    "name": "@gooddata/sdk-ui-charts",
    "version": "8.0.0-alpha.14",
    "description": "GoodData.UI SDK - Charts",
    "main": "dist/index.js",
    "repository": "https://github.com/gooddata/gooddata-ui-sdk/tree/master/libs/sdk-ui-charts",
    "author": "GoodData Corporation",
    "license": "BSD-3-Clause",
    "files": [
        "dist",
        "src",
        "styles",
        "LICENSE"
    ],
    "config": {
        "tslint": "./{__mocks__,src,stories}/{,**/}*.{ts,tsx}"
    },
    "scripts": {
        "prepublishOnly": "npm run build",
        "build": "bash scripts/build.sh",
        "build-dev": "bash scripts/build.sh --dev",
        "dev": "bash scripts/build.sh --dev-watch",
        "test": "jest --watch",
        "test-once": "jest",
        "test-ci": "JEST_JUNIT_OUTPUT=./ci/results/test-results.xml jest --ci --config jest.ci.js",
        "tslint": "tslint -p . -t verbose $npm_package_config_tslint",
        "tslint-ci": "mkdir -p ./ci/results && tslint -p . -t checkstyle -o ./ci/results/tslint-results.xml $npm_package_config_tslint",
        "prettier-check": "prettier --check '{src,stories,styles,__mocks__}/**/*.{ts,tsx,json,scss,md,yaml,html}'",
        "prettier-write": "prettier --write '{src,stories,styles,__mocks__}/**/*.{ts,tsx,json,scss,md,yaml,html}'",
        "dep-cruiser": "depcruise --validate .dependency-cruiser.js --output-type err-long src/",
        "dep-cruiser-ci": "depcruise --validate .dependency-cruiser.js --output-type err-html src/ >./ci/results/dep-cruiser.html",
        "validate": "tsc -p tsconfig.build.json --noEmit && npm run tslint && npm run prettier-check",
        "validate-ci": "npm run tslint-ci && npm run prettier-check"
    },
    "typings": "dist/index.d.ts",
    "dependencies": {
        "@ag-grid-community/all-modules": "22.1.1",
        "@ag-grid-community/react": "22.1.2",
        "@formatjs/intl-pluralrules": "~1.3.7",
        "@gooddata/goodstrap": "^66.4.0",
        "@gooddata/js-utils": "^3.4.0",
        "@gooddata/numberjs": "^3.2.3",
        "@gooddata/sdk-backend-spi": "^8.0.0-alpha.14",
        "@gooddata/sdk-model": "^8.0.0-alpha.14",
        "@gooddata/sdk-ui": "^8.0.0-alpha.14",
        "classnames": "^2.2.6",
        "custom-event": "^1.0.1",
        "date-fns": "^2.8.1",
        "downshift": "^3.4.7",
        "fixed-data-table-2": "^0.7.17",
        "highcharts": "7.1.1",
        "highcharts-grouped-categories": "^1.1.3",
        "hoist-non-react-statics": "^3.3.0",
        "http-status-codes": "^1.3.0",
        "invariant": "^2.2.2",
        "lodash": "^4.17.15",
        "moment": "^2.24.0",
        "react-day-picker": "7.3.0",
        "react-dom-factories": "^1.0.2",
        "react-intl": "^3.6.0",
        "react-measure": "^2.3.0",
        "react-responsive": "^8.0.1",
        "react-window": "^1.8.5",
        "uuid": "^3.3.3"
    },
    "peerDependencies": {
        "react": "^16.10.0",
        "react-dom": "^16.10.0"
    },
    "devDependencies": {
        "@gooddata/reference-workspace": "^8.0.0-alpha.0",
        "@gooddata/sdk-backend-mockingbird": "^8.0.0-alpha.14",
        "@gooddata/tslint-config": "1.0.0",
        "@types/classnames": "2.2.3",
        "@types/enzyme": "^3.10.3",
        "@types/enzyme-adapter-react-16": "^1.0.5",
        "@types/hoist-non-react-statics": "^3.3.1",
        "@types/invariant": "^2.2.29",
        "@types/jest": "^25.1.2",
        "@types/lodash": "^4.14.123",
        "@types/node": "^12.12.26",
        "@types/raf": "^3.4.0",
        "@types/react": "^16.9.11",
        "@types/react-dom": "^16.9.3",
        "@types/react-measure": "2.0.5",
        "@types/react-portal": "4.0.1",
        "@types/react-responsive": "^8.0.2",
        "@types/react-window": "^1.8.1",
        "@types/uuid": "^3.4.5",
        "dependency-cruiser": "^6.0.0",
        "enzyme": "^3.10.0",
        "enzyme-adapter-react-16": "^1.15.1",
        "foundation-sites": "^5.5.3",
        "full-icu": "^1.3.0",
        "jest": "^25.1.0",
        "jest-enzyme": "^7.1.2",
        "jest-junit": "^3.0.0",
        "node-sass": "^4.13.0",
        "node-sass-magic-importer": "^5.3.2",
        "prettier": "~1.19.1",
        "raf": "^3.4.1",
        "ts-jest": "^25.2.0",
        "tslint": "^5.14.0",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "3.7.4"
    }
}
