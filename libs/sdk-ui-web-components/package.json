{
    "name": "@gooddata/sdk-ui-web-components",
    "version": "8.11.0-alpha.21",
    "description": "GoodData Web Components wrapper for GoodData.UI",
    "repository": {
        "type": "git",
        "url": "https://github.com/gooddata/gooddata-ui-sdk.git",
        "directory": "libs/sdk-ui-web-components"
    },
    "author": "GoodData Corporation",
    "license": "MIT",
    "main": "esm/index.js",
    "browser": "esm/index.js",
    "module": "esm/index.js",
    "es2015": "esm/index.js",
    "typings": "esm/index.d.ts",
    "sideEffects": [
        "*.css",
        "*.svg"
    ],
    "files": [
        "esm/**/*.js",
        "esm/**/*.json",
        "esm/**/*.d.ts",
        "esm/**/*.map",
        "esm/**/*.svg",
        "esm/**/*.woff",
        "esm/**/*.eot",
        "esm/**/*.ttf",
        "bin/**/*.sh"
    ],
    "config": {
        "eslint": "-c .eslintrc.js --ext ts,tsx src/"
    },
    "scripts": {
        "clean": "rm -rf esm coverage *.log styles/css && jest --clearCache",
        "api-extractor": "mkdir -p api && [ -z \"${CI}\" ] && (api-extractor run -l) || (api-extractor run)",
        "build": "npm run clean && webpack --mode production && npm run api-extractor",
        "build-dev": "npm run clean && webpack --mode development",
        "build-analyze": "npm run clean && webpack --mode production --env analyze",
        "dev": "npm run clean && webpack --mode development --watch",
        "proxy": "node ./scripts/proxy.js",
        "test": "jest --watch",
        "test-once": "jest --maxWorkers=${JEST_MAX_WORKERS:-'45%'}",
        "test-ci": "JEST_JUNIT_OUTPUT=./ci/results/test-results.xml jest --ci --config jest.ci.js",
        "eslint": "eslint $npm_package_config_eslint",
        "eslint-ci": "mkdir -p ./ci/results && eslint -f checkstyle -o ci/results/eslint-results.xml $npm_package_config_eslint",
        "prettier-check": "prettier --check '{src,stories,styles,__mocks__}/**/*.{ts,tsx,json,scss,md,yaml,html}'",
        "prettier-write": "prettier --write '{src,stories,styles,__mocks__}/**/*.{ts,tsx,json,scss,md,yaml,html}'",
        "dep-cruiser": "depcruise --validate .dependency-cruiser.js --output-type err-long src/",
        "dep-cruiser-ci": "depcruise --validate .dependency-cruiser.js --output-type err-long src/",
        "validate": "npm run dep-cruiser && npm run eslint && npm run prettier-check",
        "validate-ci": "npm run dep-cruiser-ci && npm run eslint-ci && npm run prettier-check"
    },
    "dependencies": {
        "@gooddata/sdk-backend-bear": "^8.11.0-alpha.88",
        "@gooddata/sdk-backend-tiger": "^8.11.0-alpha.88",
        "@gooddata/sdk-backend-spi": "^8.11.0-alpha.88",
        "@gooddata/api-client-bear": "^8.11.0-alpha.88",
        "@gooddata/sdk-model": "^8.11.0-alpha.88",
        "@gooddata/sdk-ui": "^8.11.0-alpha.88",
        "@gooddata/sdk-ui-charts": "^8.11.0-alpha.88",
        "@gooddata/sdk-ui-dashboard": "^8.11.0-alpha.88",
        "@gooddata/sdk-ui-ext": "^8.11.0-alpha.88",
        "@gooddata/sdk-ui-filters": "^8.11.0-alpha.88",
        "@gooddata/sdk-ui-geo": "^8.11.0-alpha.88",
        "@gooddata/sdk-ui-kit": "^8.11.0-alpha.88",
        "@gooddata/sdk-ui-pivot": "^8.11.0-alpha.88",
        "lodash": "^4.17.19",
        "react-intl": "^5.23.0",
        "tslib": "^2.0.0",
        "ts-invariant": "^0.7.3",
        "mini-css-extract-plugin": "~2.6.1"
    },
    "peerDependencies": {
        "react": "^16.10.0 || ^17.0.0",
        "react-dom": "^16.10.0 || ^17.0.0"
    },
    "devDependencies": {
        "@babel/preset-react": "^7.7.2",
        "@gooddata/eslint-config": "^2.1.0",
        "@gooddata/sdk-backend-mockingbird": "^8.11.0-alpha.88",
        "@microsoft/api-documenter": "^7.17.0",
        "@microsoft/api-extractor": "^7.20.0",
        "@types/enzyme": "^3.10.3",
        "@types/jest": "^27.0.1",
        "@types/lodash": "^4.14.158",
        "@types/raf": "^3.4.0",
        "@types/react-dom": "^17.0.11",
        "@types/react": "^17.0.34",
        "@typescript-eslint/eslint-plugin": "^5.5.0",
        "@typescript-eslint/parser": "^5.5.0",
        "@wojtekmaj/enzyme-adapter-react-17": "^0.6.5",
        "babel-loader": "^8.0.5",
        "concurrently": "^6.0.2",
        "css-loader": "^6.7.1",
        "dependency-cruiser": "^10.1.1",
        "enzyme": "^3.10.0",
        "eslint": "^8.3.0",
        "eslint-plugin-header": "^3.0.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-jest": "^25.3.0",
        "eslint-plugin-no-only-tests": "^2.4.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react-hooks": "^4.5.0",
        "eslint-plugin-react": "^7.20.5",
        "eslint-plugin-regexp": "^1.7.0",
        "eslint-plugin-sonarjs": "^0.14.0",
        "eslint-plugin-tsdoc": "^0.2.14",
        "jest": "^27.5.1",
        "jest-enzyme": "^7.1.2",
        "jest-junit": "^3.0.0",
        "prettier": "~2.5.0",
        "raf": "^3.4.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "style-loader": "^3.3.1",
        "ts-jest": "^27.0.5",
        "ts-loader": "^8.3.0",
        "typescript": "4.0.2",
        "webpack": "^5.58.0",
        "webpack-bundle-analyzer": "^4.5.0",
        "webpack-cli": "^4.9.2",
        "source-map-loader": "^4.0.0"
    }
}
