        "eslint-plugin-react-hooks": "4.6.0",
        "eslint-plugin-regexp": "1.15.0",
        "eslint-plugin-sonarjs": "0.16.0",
        "eslint-plugin-storybook": "^9.0.17",
        "eslint-plugin-tsdoc": "0.2.14",
        "fast-glob": "3.3.3",
        "happy-dom": "18.0.1",
        "json-stable-stringify": "1.3.0",
        "lodash-es": "^4.17.21",
