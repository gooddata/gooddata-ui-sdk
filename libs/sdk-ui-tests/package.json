{
    "name": "@gooddata/sdk-ui-tests",
    "version": "8.2.0-alpha.7",
    "description": "GoodData React components test project",
    "license": "LicenseRef-LICENSE",
    "author": "GoodData Corporation",
    "repository": {
        "type": "git",
        "url": "https://github.com/gooddata/gooddata-ui-sdk.git",
        "directory": "libs/sdk-ui-tests"
    },
    "files": [
        "esm/**/*.js",
        "esm/**/*.json",
        "esm/**/*.d.ts",
        "esm/**/*.map",
        "backstop",
        "stories",
        ".storybook"
    ],
    "type": "module",
    "browser": "./esm/index.js",
    "types": "./esm/index.d.ts",
    "exports": "./esm/index.js",
    "scripts": {
        "_phase:build": "npm run build",
        "_phase:test": "npm run test-once",
        "_phase:validate": "npm run validate",
        "build": "npm-run-all -p build-check build-ts",
        "build-check": "tsc",
        "build-storybook": "export NODE_OPTIONS=\"--max_old_space_size=8192\" && npm run build && storybook build -c .storybook -o dist-storybook --quiet",
        "build-ts": "tsc -p tsconfig.build.json",
        "clean": "../../common/scripts/clean-command-state.sh && rm -rf ci esm dist coverage *.log tsconfig.tsbuildinfo",
        "dep-cruiser": "depcruise --validate .dependency-cruiser.cjs --output-type err-long src/",
        "eslint": "eslint .",
        "eslint-fix": "eslint . --fix",
        "neobackstop-approve-docker": "npm run neobackstop-prepare-docker && bash neobackstop/run-neobackstop-compose.sh approve",
        "neobackstop-approve-local": "npm run neobackstop-prepare-local && bash neobackstop/run-neobackstop-compose.sh approve",
        "neobackstop-prepare-docker": "npm run story-generator && npm run build-storybook && npm run scenario-extractor-docker",
        "neobackstop-prepare-local": "npm run story-generator && npm run build-storybook && rm -rf neobackstop/debug/dist-storybook && cp -r dist-storybook neobackstop/debug && npm run scenario-extractor-local",
        "neobackstop-test-docker": "npm run neobackstop-prepare-docker && bash neobackstop/run-neobackstop-compose.sh test",
        "neobackstop-test-local": "npm run neobackstop-prepare-local && bash neobackstop/run-neobackstop-compose.sh test",
        "populate-ref": "GDC_STORE_DEFS=\"../../tools\" vitest run tests/smoke-and-capture/",
        "oxfmt-check": "oxfmt --check .",
        "oxfmt-write": "oxfmt .",
        "scenario-extractor-docker": "DOCKER=true vite-node neobackstop/scenario-extractor.ts",
        "scenario-extractor-local": "vite-node neobackstop/scenario-extractor.ts",
        "story-generator": "vite-node neobackstop/story-generator.ts",
        "storybook": "storybook dev -p 9001 -c .storybook",
        "storybook-serve": "bash stories/serve-storybook.sh",
        "stylelint": "stylelint '**/*.scss'",
        "stylelint-fix": "stylelint '**/*.scss' --fix",
        "test": "vitest watch",
        "test-once": "npm run story-generator && npm run scenario-extractor-local && vitest --run",
        "validate": "npm run lint && npm run dep-cruiser && npm run stylelint && npm run oxfmt-check",
        "lint": "npm run oxlint && npm run eslint",
        "lint-fix": "npm run oxlint-fix && npm run eslint-fix",
        "oxlint": "oxlint .",
        "oxlint-fix": "oxlint . --fix"
    },
    "devDependencies": {
        "@gooddata/eslint-config": "workspace:*",
        "@gooddata/mock-handling": "workspace:*",
        "@gooddata/oxlint-config": "workspace:*",
        "@gooddata/reference-workspace": "workspace:*",
        "@gooddata/sdk-backend-base": "workspace:*",
        "@gooddata/sdk-backend-mockingbird": "workspace:*",
        "@gooddata/sdk-backend-spi": "workspace:*",
        "@gooddata/sdk-model": "workspace:*",
        "@gooddata/sdk-ui": "workspace:*",
        "@gooddata/sdk-ui-charts": "workspace:*",
        "@gooddata/sdk-ui-dashboard": "workspace:*",
        "@gooddata/sdk-ui-ext": "workspace:*",
        "@gooddata/sdk-ui-filters": "workspace:*",
        "@gooddata/sdk-ui-gen-ai": "workspace:*",
        "@gooddata/sdk-ui-geo": "workspace:*",
        "@gooddata/sdk-ui-kit": "workspace:*",
        "@gooddata/sdk-ui-pivot": "workspace:*",
        "@gooddata/sdk-ui-semantic-search": "workspace:*",
        "@gooddata/sdk-ui-theme-provider": "workspace:*",
        "@gooddata/sdk-ui-web-components": "workspace:*",
        "@gooddata/stylelint-config": "workspace:*",
        "@storybook/addon-a11y": "^10.1.11",
        "@storybook/builder-vite": "^10.1.11",
        "@storybook/cli": "^10.1.11",
        "@storybook/react-vite": "^10.1.11",
        "@testing-library/dom": "10.4.1",
        "@testing-library/react": "16.3.0",
        "@types/lodash-es": "^4.17.12",
        "@types/node": "^22.13.0",
        "@types/react": "19.1.11",
        "@types/react-dom": "19.1.7",
        "@types/spark-md5": "^3.0.1",
        "@typescript-eslint/eslint-plugin": "8.52.0",
        "@typescript-eslint/parser": "8.52.0",
        "@vitest/eslint-plugin": "1.6.6",
        "dependency-cruiser": "^14.1.2",
        "eslint": "^9.39.2",
        "eslint-import-resolver-typescript": "4.4.4",
        "eslint-plugin-headers": "1.3.3",
        "eslint-plugin-import-esm": "1.2.1",
        "eslint-plugin-import-x": "4.16.1",
        "eslint-plugin-jsdoc": "62.1.0",
        "eslint-plugin-no-barrel-files": "1.2.2",
        "eslint-plugin-no-only-tests": "3.3.0",
        "eslint-plugin-react": "7.37.5",
        "eslint-plugin-react-hooks": "5.2.0",
        "eslint-plugin-sonarjs": "3.0.6",
        "eslint-plugin-storybook": "10.1.11",
        "fast-glob": "3.3.3",
        "happy-dom": "18.0.1",
        "json-stable-stringify": "1.3.0",
        "lodash-es": "^4.17.23",
        "mapbox-gl": "^2.6.1",
        "moment": "^2.29.4",
        "npm-run-all": "^4.1.5",
        "oxfmt": "0.27.0",
        "oxlint": "^1.43.0",
        "react": "19.1.1",
        "react-dom": "19.1.1",
        "react-intl": "7.1.11",
        "react-responsive": "^10.0.1",
        "require-from-string": "^2.0.2",
        "rolldown": "1.0.0-rc.1",
        "spark-md5": "^3.0.0",
        "storybook": "^10.1.11",
        "stylelint": "^16.26.1",
        "ts-invariant": "^0.7.5",
        "tslib": "2.8.1",
        "typescript": "5.9.3",
        "uuid": "^11.1.0",
        "vite": "8.0.0-beta.10",
        "vite-node": "^5.2.0",
        "vite-plugin-static-copy": "^3.1.1",
        "vitest": "4.0.8"
    },
    "peerDependencies": {
        "@gooddata/sdk-ui": "workspace:*",
        "@gooddata/sdk-ui-charts": "workspace:*",
        "@gooddata/sdk-ui-ext": "workspace:*",
        "@gooddata/sdk-ui-filters": "workspace:*",
        "@gooddata/sdk-ui-gen-ai": "workspace:*",
        "@gooddata/sdk-ui-geo": "workspace:*",
        "@gooddata/sdk-ui-kit": "workspace:*",
        "@gooddata/sdk-ui-pivot": "workspace:*",
        "@gooddata/sdk-ui-semantic-search": "workspace:*",
        "@gooddata/sdk-ui-theme-provider": "workspace:*",
        "@gooddata/sdk-ui-web-components": "workspace:*",
        "react": "^18.0.0 || ^19.0.0",
        "react-dom": "^18.0.0 || ^19.0.0",
        "typescript": ">= 2.5.3"
    }
}
