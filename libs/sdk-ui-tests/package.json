{
    "name": "@gooddata/sdk-ui-tests",
    "version": "8.2.0-alpha.7",
    "description": "GoodData React components test project",
    "repository": {
        "type": "git",
        "url": "https://github.com/gooddata/gooddata-ui-sdk.git",
        "directory": "libs/sdk-ui-tests"
    },
    "license": "LicenseRef-LICENSE",
    "author": "GoodData Corporation",
    "type": "module",
    "exports": "./esm/index.js",
    "browser": "./esm/index.js",
    "types": "./esm/index.d.ts",
    "files": [
        "esm/**/*.js",
        "esm/**/*.json",
        "esm/**/*.d.ts",
        "esm/**/*.map",
        "backstop",
        "stories",
        ".storybook"
    ],
    "scripts": {
        "_phase:build": "npm run build",
        "_phase:test": "npm run test-once",
        "_phase:validate": "npm run validate",
        "backstop-approve": "bash backstop/run-backstop-compose.sh approve",
        "backstop-prepare": "npm run story-generator && npm run build-storybook && npm run story-extractor",
        "backstop-reference": "npm run backstop-prepare && bash backstop/run-backstop-compose.sh reference",
        "backstop-test": "npm run backstop-prepare && bash backstop/run-backstop-compose.sh test",
        "build": "tsc -p tsconfig.build.json",
        "build-storybook": "export NODE_OPTIONS=\"--max_old_space_size=8192\" && npm run build && storybook build -c .storybook -o dist-storybook --quiet",
        "clean": "../../common/scripts/clean-command-state.sh && rm -rf ci esm dist coverage *.log tsconfig.tsbuildinfo",
        "dep-cruiser": "depcruise --validate .dependency-cruiser.cjs --output-type err-long src/",
        "dep-cruiser-ci": "depcruise --validate .dependency-cruiser.cjs --output-type err-long src/",
        "eslint": "eslint -c .eslintrc.cjs stories tests src scenarios",
        "eslint-ci": "mkdir -p ./ci/results && eslint -f checkstyle -o ci/results/eslint-results.xml -c .eslintrc.cjs src/",
        "eslint-fix": "eslint -c .eslintrc.cjs stories tests src scenarios --fix",
        "populate-ref": "GDC_STORE_DEFS=\"../../tools\" vitest run tests/smoke-and-capture/",
        "prettier-check": "prettier --check '{src,stories,scenarios,tests,styles,__mocks__}/**/*.{ts,tsx,json,scss,md,yaml,html}'",
        "prettier-write": "prettier --write '{src,stories,scenarios,tests,styles,__mocks__}/**/*.{ts,tsx,json,scss,md,yaml,html}'",
        "prettier-write-generated": "prettier --write --ignore-path=\"\" 'stories/visual-regression/**/*.generated.stories.tsx' > /dev/null 2>&1",
        "story-extractor": "vite-node backstop/story-extractor.ts",
        "story-generator": "vite-node backstop/story-generator.ts && npm run prettier-write-generated && npm run eslint-fix",
        "storybook": "storybook dev -p 9001 -c .storybook",
        "storybook-serve": "bash stories/serve-storybook.sh",
        "stylelint": "stylelint '**/*.scss'",
        "stylelint-ci": "stylelint '**/*.scss' --custom-formatter=node_modules/stylelint-checkstyle-formatter > ./ci/results/stylelint-results.xml",
        "stylelint-fix": "stylelint '**/*.scss' --fix",
        "test": "vitest watch",
        "test-ci": "npm run story-generator && npm run story-extractor && vitest --run",
        "test-once": "npm run story-generator && npm run story-extractor && vitest --run",
        "validate": "tsc && export BROWSERSLIST_IGNORE_OLD_DATA=true && npm run dep-cruiser && npm run eslint && npm run stylelint && npm run prettier-check"
    },
    "devDependencies": {
        "@babel/core": "^7.23.5",
        "@babel/plugin-proposal-export-default-from": "^7.23.3",
        "@babel/preset-env": "^7.23.5",
        "@babel/preset-react": "^7.23.3",
        "@babel/preset-typescript": "^7.23.3",
        "@babel/runtime": "^7.23.5",
        "@gooddata/eslint-config": "workspace:*",
        "@gooddata/mock-handling": "workspace:*",
        "@gooddata/reference-workspace": "workspace:*",
        "@gooddata/sdk-backend-base": "workspace:*",
        "@gooddata/sdk-backend-mockingbird": "workspace:*",
        "@gooddata/sdk-backend-spi": "workspace:*",
        "@gooddata/sdk-model": "workspace:*",
        "@gooddata/sdk-ui": "workspace:*",
        "@gooddata/sdk-ui-charts": "workspace:*",
        "@gooddata/sdk-ui-dashboard": "workspace:*",
        "@gooddata/sdk-ui-ext": "workspace:*",
        "@gooddata/sdk-ui-filters": "workspace:*",
