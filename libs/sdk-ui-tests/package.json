{
    "name": "@gooddata/sdk-ui-tests",
    "version": "8.0.0-alpha.0",
    "description": "GoodData React components test project",
    "main": "dist/index.js",
    "repository": "https://github.com/gooddata/gooddata-ui-sdk/tree/master/libs/sdk-ui-tests",
    "author": "GoodData Corporation",
    "license": "BSD-3-Clause",
    "files": [
        "dist",
        "src",
        "LICENSE"
    ],
    "config": {
        "tslint": "./{__mocks__,src,stories}/{,**/}*.{ts,tsx}"
    },
    "scripts": {
        "dev": "tsc --watch",
        "build": "rm -rf dist && tsc -p tsconfig.build.json",
        "test": "jest --watch",
        "test-once": "jest && npm run story-extractor",
        "test-ci": "npm run story-extractor && JEST_JUNIT_OUTPUT=./ci/results/test-results.xml jest --ci --config jest.ci.js",
        "tslint": "tslint -p . -t verbose $npm_package_config_tslint",
        "tslint-ci": "mkdir -p ./ci/results && tslint -p . -t checkstyle -o ./ci/results/tslint-results.xml $npm_package_config_tslint",
        "populate-ref": "GDC_STORE_DEFS=\"../../tools/reference-workspace/src/recordings/uiTestScenarios\" jest --config \"smoke-and-capture.config.js\" --watchAll=false",
        "story-extractor": "jest --config \"story-extractor.config.js\" --watchAll=false",
        "prettier-check": "prettier --check '{src,stories,styles,__mocks__}/**/*.{ts,tsx,json,scss,md,yaml,html}'",
        "prettier-write": "prettier --write '{src,stories,styles,__mocks__}/**/*.{ts,tsx,json,scss,md,yaml,html}'",
        "validate": "tsc -p tsconfig.build.json --noEmit && npm run tslint && npm run prettier-check",
        "validate-ci": "npm run tslint-ci && npm run prettier-check",
        "storybook": "npm run build && start-storybook -p 9001 -c .storybook",
        "build-storybook": "npm run build && build-storybook -c .storybook -o dist-storybook",
        "backstop-test": "npm run story-extractor && bash backstop/run-backstop.sh test",
        "backstop-approve": "npm run story-extractor && bash backstop/run-backstop.sh approve",
        "backstop-reference": "npm run build-storybook && npm run story-extractor && bash backstop/run-backstop.sh reference",
        "backstop-reference-nobuild": "npm run story-extractor && bash backstop/run-backstop.sh reference"
    },
    "typings": "dist/index.d.ts",
    "dependencies": {
        "@babel/core": "^7.7.2",
        "@babel/plugin-proposal-export-default-from": "^7.7.4",
        "@babel/plugin-transform-typescript": "^7.7.4",
        "@babel/preset-env": "^7.7.4",
        "@babel/preset-react": "^7.7.4",
        "@babel/runtime": "^7.7.2",
        "@gooddata/numberjs": "^3.2.3",
        "@gooddata/reference-workspace": "^8.0.0-alpha.0",
        "@gooddata/sdk-backend-mockingbird": "^8.0.0-alpha.14",
        "@gooddata/sdk-backend-spi": "^8.0.0-alpha.14",
        "@gooddata/sdk-model": "^8.0.0-alpha.14",
        "@storybook/addon-actions": "^5.2.6",
        "@storybook/addon-options": "^5.2.6",
        "@storybook/addons": "^5.2.6",
        "@storybook/react": "^5.2.6",
        "@storybook/theming": "^5.2.6",
        "babel-loader": "^8.0.5",
        "babel-plugin-require-context-hook": "^1.0.0",
        "babel-runtime": "^6.26.0",
        "core-js": "^2.6.10",
        "enzyme": "^3.10.0",
        "enzyme-adapter-react-16": "^1.15.1",
        "jest": "^25.1.0",
        "jest-junit": "^3.0.0",
        "lodash": "^4.17.15",
        "spark-md5": "^3.0.0",
        "ts-invariant": "^0.4.4",
        "ts-jest": "^25.2.0",
        "ts-loader": "^6.2.1"
    },
    "peerDependencies": {
        "@gooddata/sdk-ui": "^8.0.0-alpha.14",
        "@gooddata/sdk-ui-ext": "^8.0.0-alpha.14",
        "@gooddata/sdk-ui-filters": "^8.0.0-alpha.14",
        "@gooddata/sdk-ui-pivot": "^8.0.0-alpha.14",
        "react": "^16.10.0",
        "react-dom": "^16.10.0",
        "typescript": ">= 2.5.3"
    },
    "devDependencies": {
        "@gooddata/sdk-ui": "^8.0.0-alpha.14",
        "@gooddata/sdk-ui-ext": "^8.0.0-alpha.14",
        "@gooddata/sdk-ui-filters": "^8.0.0-alpha.14",
        "@gooddata/sdk-ui-pivot": "^8.0.0-alpha.14",
        "@gooddata/tslint-config": "1.0.0",
        "@types/enzyme": "^3.10.3",
        "@types/enzyme-adapter-react-16": "^1.0.5",
        "@types/jest": "^25.1.2",
        "@types/lodash": "^4.14.123",
        "@types/node": "^12.12.26",
        "@types/react": "^16.9.11",
        "@types/react-dom": "^16.9.3",
        "@types/spark-md5": "^3.0.1",
        "prettier": "~1.19.1",
        "react": "^16.10.0",
        "react-dom": "^16.10.0",
        "ts-node": "^8.4.1",
        "tslint": "^5.14.0",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "3.7.4"
    }
}
