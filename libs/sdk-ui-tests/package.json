        "@storybook/preset-scss": "^1.0.3",
        "@storybook/react": "^9.0.17",
        "@storybook/react-vite": "^9.0.17",
        "@testing-library/dom": "10.4.0",
        "@testing-library/react": "16.3.0",
        "@types/lodash-es": "^4.17.12",
        "@types/node": "^22.13.0",
        "@types/react": "19.1.11",
