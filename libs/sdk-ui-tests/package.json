        "eslint-config-prettier": "10.1.8",
        "eslint-plugin-eslint-comments": "3.2.0",
        "eslint-plugin-header": "3.1.1",
        "eslint-plugin-import": "2.32.0",
        "eslint-plugin-import-esm": "1.2.1",
        "eslint-plugin-no-only-tests": "2.6.0",
