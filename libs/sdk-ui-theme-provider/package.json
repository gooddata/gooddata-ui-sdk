{
    "name": "@gooddata/sdk-ui-theme-provider",
    "version": "11.20.0-alpha.0",
    "description": "GoodData SDK - Theme provider",
    "license": "MIT",
    "author": "GoodData Corporation",
    "repository": {
        "type": "git",
        "url": "https://github.com/gooddata/gooddata-ui-sdk.git",
        "directory": "libs/sdk-ui-theme-provider"
    },
    "files": [
        "esm/**/*.js",
        "esm/**/*.json",
        "esm/**/*.d.ts",
        "esm/**/*.map"
    ],
    "type": "module",
    "sideEffects": false,
    "types": "./esm/index.d.ts",
    "exports": {
        ".": "./esm/index.js",
        "./node": "./esm/node.js",
        "./internal": "./esm/internal.js"
    },
    "scripts": {
        "_phase:build": "npm run build",
        "_phase:test": "npm run test-once",
        "_phase:validate": "npm run validate",
        "api-extractor": "mkdir -p api && [ -z \"${CI}\" ] && (api-extractor run -l) || (api-extractor run)",
        "build": "npm-run-all -p build-check build-ts && npm run api-extractor",
        "build-check": "tsc",
        "build-ts": "tsc -p tsconfig.build.json",
        "clean": "../../common/scripts/clean-command-state.sh && rm -rf ci dist esm coverage *.log tsconfig.tsbuildinfo",
        "dep-cruiser": "depcruise --validate .dependency-cruiser.cjs --output-type err-long src/",
        "eslint": "eslint .",
        "eslint-fix": "eslint . --fix",
        "oxfmt-check": "oxfmt --check .",
        "oxfmt-write": "oxfmt .",
        "test": "vitest watch",
        "test-once": "vitest run",
        "validate": "npm run lint && npm run dep-cruiser && npm run oxfmt-check",
        "validate-esm": "node --input-type=module --eval 'import \"@gooddata/sdk-ui-theme-provider\"'",
        "lint": "npm run oxlint && npm run eslint",
        "lint-fix": "npm run oxlint-fix && npm run eslint-fix",
        "oxlint": "oxlint . --type-aware",
        "oxlint-fix": "oxlint . --type-aware --fix"
    },
    "dependencies": {
        "@gooddata/sdk-backend-spi": "workspace:*",
        "@gooddata/sdk-model": "workspace:*",
        "@gooddata/sdk-ui": "workspace:*",
        "@gooddata/util": "workspace:*",
        "chalk": "^4.1.1",
        "glob": "^11.0.0",
        "lodash-es": "^4.17.23",
        "polished": "^3.6.7",
        "postcss": "^8.4.49",
        "postcss-value-parser": "^4.2.0",
        "ts-invariant": "^0.7.5",
        "tslib": "2.8.1"
    },
    "devDependencies": {
        "@gooddata/eslint-config": "workspace:*",
        "@gooddata/oxlint-config": "workspace:*",
        "@gooddata/reference-workspace": "workspace:*",
        "@gooddata/sdk-backend-mockingbird": "workspace:*",
        "@microsoft/api-documenter": "^7.17.0",
        "@microsoft/api-extractor": "^7.55.2",
        "@testing-library/dom": "10.4.1",
        "@testing-library/react": "16.3.0",
        "@types/lodash-es": "^4.17.12",
        "@types/node": "^22.13.0",
        "@types/raf": "^3.4.0",
        "@types/react": "19.1.11",
        "@types/react-dom": "19.1.7",
        "@typescript-eslint/eslint-plugin": "8.52.0",
        "@typescript-eslint/parser": "8.52.0",
        "@vitest/eslint-plugin": "1.6.6",
        "dependency-cruiser": "^14.1.2",
        "eslint": "^9.39.2",
        "eslint-import-resolver-typescript": "4.4.4",
        "eslint-plugin-headers": "1.3.3",
        "eslint-plugin-import-esm": "1.2.1",
        "eslint-plugin-import-x": "4.16.1",
        "eslint-plugin-jsdoc": "62.1.0",
        "eslint-plugin-no-barrel-files": "1.2.2",
        "eslint-plugin-no-only-tests": "3.3.0",
        "eslint-plugin-react": "7.37.5",
        "eslint-plugin-react-hooks": "5.2.0",
        "eslint-plugin-sonarjs": "3.0.6",
        "happy-dom": "18.0.1",
        "npm-run-all": "^4.1.5",
        "oxfmt": "0.27.0",
        "oxlint": "^1.43.0",
        "oxlint-tsgolint": "0.11.4",
        "raf": "^3.4.1",
        "react": "19.1.1",
        "react-dom": "19.1.1",
        "typescript": "5.9.3",
        "vitest": "4.0.8"
    },
    "peerDependencies": {
        "react": "^18.0.0 || ^19.0.0",
        "react-dom": "^18.0.0 || ^19.0.0"
    }
}
