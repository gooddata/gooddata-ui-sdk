{
    "name": "@gooddata/sdk-ui",
    "version": "8.0.0-beta.18",
    "description": "GoodData.UI SDK - Core",
    "main": "dist/index.js",
    "repository": "https://github.com/gooddata/gooddata-ui-sdk/tree/master/libs/sdk-ui",
    "author": "GoodData Corporation",
    "license": "LicenseRef-LICENSE",
    "files": [
        "dist",
        "src",
        "styles",
        "LICENSE"
    ],
    "config": {
        "tslint": "./{__mocks__,src}/{,**/}*.{ts,tsx}"
    },
    "scripts": {
        "clean": "rm -rf ci dist coverage *.log && jest --clearCache",
        "prepublishOnly": "npm run build",
        "build": "bash scripts/build.sh",
        "build-dev": "bash scripts/build.sh --dev",
        "dev": "bash scripts/build.sh --dev-watch",
        "test": "jest --watch",
        "test-once": "jest",
        "test-ci": "JEST_JUNIT_OUTPUT=./ci/results/test-results.xml jest --ci --config jest.ci.js",
        "tslint": "tslint -p . -t verbose $npm_package_config_tslint",
        "tslint-ci": "mkdir -p ./ci/results && tslint -p . -t checkstyle -o ./ci/results/tslint-results.xml $npm_package_config_tslint",
        "prettier-check": "prettier --check '{src,stories,styles,__mocks__}/**/*.{ts,tsx,json,scss,md,yaml,html}'",
        "prettier-write": "prettier --write '{src,stories,styles,__mocks__}/**/*.{ts,tsx,json,scss,md,yaml,html}'",
        "dep-cruiser": "depcruise --validate .dependency-cruiser.js --output-type err-long src/",
        "dep-cruiser-ci": "depcruise --validate .dependency-cruiser.js --output-type err-long src/",
        "validate": "npm run dep-cruiser && npm run tslint && npm run prettier-check",
        "validate-ci": "npm run dep-cruiser-ci && npm run tslint-ci && npm run prettier-check"
    },
    "typings": "dist/index.d.ts",
    "dependencies": {
        "@formatjs/intl-pluralrules": "~1.3.7",
        "@gooddata/goodstrap": "^68.15.0",
        "@gooddata/js-utils": "^3.4.0",
        "@gooddata/numberjs": "^3.2.4",
        "@gooddata/sdk-backend-spi": "^8.0.0-beta.18",
        "@gooddata/sdk-model": "^8.0.0-beta.18",
        "custom-event": "^1.0.1",
        "hoist-non-react-statics": "^3.3.0",
        "http-status-codes": "^1.3.0",
        "lodash": "^4.17.15",
        "react-intl": "^3.6.0",
        "ts-invariant": "^0.4.4",
        "tslib": "^2.0.0"
    },
    "peerDependencies": {
        "react": "^16.10.0",
        "react-dom": "^16.10.0"
    },
    "devDependencies": {
        "@gooddata/reference-workspace": "^8.0.0-beta.18",
        "@gooddata/sdk-backend-mockingbird": "^8.0.0-beta.18",
        "@gooddata/tslint-config": "1.0.0",
        "@types/enzyme": "^3.10.3",
        "@types/enzyme-adapter-react-16": "^1.0.5",
        "@types/hoist-non-react-statics": "^3.3.1",
        "@types/jest": "^25.1.2",
        "@types/lodash": "^4.14.123",
        "@types/node": "^12.12.26",
        "@types/raf": "^3.4.0",
        "@types/react": "^16.9.11",
        "@types/react-dom": "^16.9.3",
        "dependency-cruiser": "^8.0.1",
        "enzyme": "^3.10.0",
        "enzyme-adapter-react-16": "^1.15.1",
        "full-icu": "^1.3.0",
        "jest": "^25.1.0",
        "jest-enzyme": "^7.1.2",
        "jest-junit": "^3.0.0",
        "prettier": "~2.0.5",
        "raf": "^3.4.1",
        "ts-jest": "^25.2.0",
        "tslint": "^5.14.0",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "3.9.5"
    }
}
