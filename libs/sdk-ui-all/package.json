{
    "name": "@gooddata/sdk-ui-all",
    "version": "8.6.0-alpha.41",
    "author": "GoodData",
    "description": "GoodData SDK - All-In-One",
    "repository": {
        "type": "git",
        "url": "https://github.com/gooddata/gooddata-ui-sdk.git",
        "directory": "libs/sdk-ui-all"
    },
    "license": "LicenseRef-LICENSE",
    "main": "dist/index.js",
    "module": "esm/index.js",
    "browser": "dist/index.js",
    "typings": "esm/index.d.ts",
    "sideEffects": false,
    "files": [
        "dist/**/*.js",
        "dist/**/*.json",
        "dist/**/*.d.ts",
        "dist/**/*.map",
        "esm/**/*.js",
        "esm/**/*.json",
        "esm/**/*.d.ts",
        "esm/**/*.map",
        "styles"
    ],
    "config": {
        "eslint": "-c .eslintrc.js --ext ts src/"
    },
    "scripts": {
        "clean": "rm -rf ci dist esm coverage *.log && jest --clearCache",
        "dev": "tsc -p tsconfig.dev.json --watch",
        "build": "concurrently \"npm run build-cjs\" \"npm run build-esm\" && npm run api-extractor",
        "build-cjs": "tsc -p tsconfig.build.json",
        "build-esm": "tsc -p tsconfig.build.esm.json",
        "prepack": "cp -f ../../NOTICE ./NOTICE",
        "api-extractor": "mkdir -p api && [ -z \"${CI}\" ] && (api-extractor run -l) || (api-extractor run)",
        "test": "jest --watch",
        "test-once": "jest --passWithNoTests",
        "test-ci": "JEST_JUNIT_OUTPUT=./ci/results/test-results.xml jest --passWithNoTests --ci --config jest.ci.js",
        "eslint": "eslint $npm_package_config_eslint",
        "eslint-ci": "mkdir -p ./ci/results && eslint -f checkstyle -o ci/results/eslint-results.xml $npm_package_config_eslint",
        "prettier-check": "prettier --check '{src,test}/**/*.{ts,tsx,json,scss,md,yaml,html}'",
        "prettier-write": "prettier --write '{src,test}/**/*.{ts,tsx,json,scss,md,yaml,html}'",
        "dep-cruiser": "depcruise --validate .dependency-cruiser.js --output-type err-long src/",
        "dep-cruiser-ci": "mkdir -p ci/results && depcruise --validate .dependency-cruiser.js --output-type err-html src/ >./ci/results/dep-cruiser.html",
        "validate": "npm run dep-cruiser && npm run eslint && npm run prettier-check",
        "validate-ci": "npm run dep-cruiser-ci && npm run eslint-ci && npm run prettier-check"
    },
    "dependencies": {
        "@gooddata/sdk-backend-spi": "^8.6.0-alpha.41",
        "@gooddata/sdk-model": "^8.6.0-alpha.41",
        "@gooddata/sdk-ui": "^8.6.0-alpha.41",
        "@gooddata/sdk-ui-charts": "^8.6.0-alpha.41",
        "@gooddata/sdk-ui-ext": "^8.6.0-alpha.41",
        "@gooddata/sdk-ui-filters": "^8.6.0-alpha.41",
        "@gooddata/sdk-ui-geo": "^8.6.0-alpha.41",
        "@gooddata/sdk-ui-pivot": "^8.6.0-alpha.41",
        "tslib": "^2.0.0"
    },
    "devDependencies": {
        "@gooddata/eslint-config": "^2.1.0",
        "@microsoft/api-documenter": "^7.12.13",
        "@microsoft/api-extractor": "^7.18.4",
        "@types/jest": "^26.0.12",
        "@typescript-eslint/eslint-plugin": "^4.14.2",
        "@typescript-eslint/parser": "^4.14.2",
        "concurrently": "^6.0.2",
        "dependency-cruiser": "^9.9.5",
        "eslint": "^7.5.0",
        "eslint-plugin-header": "^3.0.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-jest": "^24.1.3",
        "eslint-plugin-no-only-tests": "^2.4.0",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-sonarjs": "^0.9.1",
        "jest": "^26.4.2",
        "jest-junit": "^3.0.0",
        "prettier": "~2.3.0",
        "ts-jest": "^26.3.0",
        "typescript": "4.0.2"
    }
}
