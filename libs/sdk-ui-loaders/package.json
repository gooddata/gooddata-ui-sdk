{
    "name": "@gooddata/sdk-ui-loaders",
    "version": "11.19.0-alpha.8",
    "description": "GoodData SDK Runtime Component Loaders",
    "license": "LicenseRef-LICENSE",
    "author": "GoodData Corporation",
    "repository": {
        "type": "git",
        "url": "https://github.com/gooddata/gooddata-ui-sdk.git",
        "directory": "libs/sdk-ui-loaders"
    },
    "files": [
        "esm/**/*.js",
        "esm/**/*.json",
        "esm/**/*.d.ts",
        "esm/**/*.map",
        "esm/**/*.svg",
        "NOTICE"
    ],
    "type": "module",
    "sideEffects": [
        "*.css",
        "*.svg"
    ],
    "browser": "./esm/index.js",
    "types": "./esm/index.d.ts",
    "exports": "./esm/index.js",
    "scripts": {
        "_phase:build": "npm run build",
        "_phase:validate": "npm run validate",
        "api-extractor": "mkdir -p api && [ -z \"${CI}\" ] && (api-extractor run -l) || (api-extractor run)",
        "build": "npm-run-all -p build-check build-ts && npm run api-extractor",
        "build-check": "tsc",
        "build-ts": "tsc -p tsconfig.build.json",
        "clean": "../../common/scripts/clean-command-state.sh && rm -rf ci dist esm coverage *.log styles/css tsconfig.tsbuildinfo",
        "dep-cruiser": "depcruise --validate .dependency-cruiser.cjs --output-type err-long src/",
        "eslint": "eslint .",
        "eslint-fix": "eslint . --fix",
        "prepack": "cp -f ../../NOTICE ./NOTICE",
        "oxfmt-check": "oxfmt --check .",
        "oxfmt-write": "oxfmt .",
        "validate": "npm run eslint && npm run dep-cruiser && npm run oxfmt-check",
        "validate-esm": "node --input-type=module --eval 'import \"@gooddata/sdk-ui-loaders\"'"
    },
    "dependencies": {
        "@gooddata/sdk-backend-spi": "workspace:*",
        "@gooddata/sdk-model": "workspace:*",
        "@gooddata/sdk-ui": "workspace:*",
        "@gooddata/sdk-ui-dashboard": "workspace:*",
        "json-stable-stringify": "1.3.0",
        "lodash-es": "^4.17.23",
        "react-intl": "7.1.11",
        "semver": "^7.3.5",
        "ts-invariant": "^0.7.5",
        "tslib": "2.8.1"
    },
    "devDependencies": {
        "@gooddata/eslint-config": "workspace:*",
        "@microsoft/api-documenter": "^7.17.0",
        "@microsoft/api-extractor": "^7.55.2",
        "@types/lodash-es": "^4.17.12",
        "@types/node": "^22.13.0",
        "@types/raf": "^3.4.0",
        "@types/react": "19.1.11",
        "@types/react-dom": "19.1.7",
        "@types/semver": "^7.3.9",
        "@typescript-eslint/eslint-plugin": "8.52.0",
        "@typescript-eslint/parser": "8.52.0",
        "dependency-cruiser": "^14.1.2",
        "eslint": "^9.39.2",
        "eslint-import-resolver-typescript": "4.4.4",
        "eslint-plugin-header": "3.1.1",
        "eslint-plugin-import-esm": "1.2.1",
        "eslint-plugin-import-x": "4.16.1",
        "eslint-plugin-jsdoc": "62.1.0",
        "eslint-plugin-no-barrel-files": "1.2.2",
        "eslint-plugin-no-only-tests": "3.3.0",
        "eslint-plugin-react": "7.37.5",
        "eslint-plugin-react-hooks": "5.2.0",
        "eslint-plugin-sonarjs": "3.0.6",
        "npm-run-all": "^4.1.5",
        "oxfmt": "0.27.0",
        "raf": "^3.4.1",
        "react": "19.1.1",
        "react-dom": "19.1.1",
        "typescript": "5.9.3"
    },
    "peerDependencies": {
        "react": "^18.0.0 || ^19.0.0",
        "react-dom": "^18.0.0 || ^19.0.0"
    }
}
