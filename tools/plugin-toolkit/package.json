{
    "name": "@gooddata/plugin-toolkit",
    "version": "11.23.0-alpha.1",
    "description": "GoodData Set of Tools for working with Plugins",
    "license": "LicenseRef-LICENSE",
    "author": "GoodData",
    "repository": {
        "type": "git",
        "url": "https://github.com/gooddata/gooddata-ui-sdk.git",
        "directory": "tools/plugin-toolkit"
    },
    "bin": {
        "gdc-plugins": "esm/index.js"
    },
    "files": [
        "esm/**/*.js",
        "esm/**/*.json",
        "esm/**/*.d.ts",
        "esm/**/*.map",
        "esm/**/*.tgz",
        "NOTICE"
    ],
    "type": "module",
    "sideEffects": false,
    "browser": "esm/index.js",
    "types": "./esm/index.d.ts",
    "exports": "./esm/index.js",
    "scripts": {
        "_phase:build": "npm run build",
        "_phase:post-build": "npm run build-dynamic-files",
        "_phase:test": "npm run build-dynamic-files && npm run test-once",
        "_phase:validate": "npm run build-dynamic-files && npm run validate",
        "build": "bash scripts/build.sh && tsgo",
        "build-dynamic-files": "bash scripts/build.sh --genFilesOnly",
        "clean": "../../common/scripts/clean-command-state.sh && rm -rf ci dist esm build coverage *.log tsconfig.tsbuildinfo",
        "dep-cruiser": "depcruise --validate .dependency-cruiser.cjs --output-type err-long src/",
        "prepack": "cp -f ../../NOTICE ./NOTICE",
        "prepublishOnly": "npm run build",
        "format-check": "oxfmt --check .",
        "format-write": "oxfmt .",
        "test": "vitest watch",
        "test-once": "vitest run",
        "validate": "npm run dep-cruiser && npm run lint && npm run format-check",
        "lint": "oxlint . --type-aware --quiet && eslint src",
        "lint-fix": "oxlint . --type-aware --quiet --fix && eslint src --fix"
    },
    "dependencies": {
        "@babel/cli": "^7.23.4",
        "@babel/core": "7.28.4",
        "@babel/preset-env": "7.24.7",
        "@babel/preset-typescript": "^7.23.3",
        "@gooddata/sdk-backend-spi": "workspace:*",
        "@gooddata/sdk-backend-tiger": "workspace:*",
        "@gooddata/sdk-model": "workspace:*",
        "axios": "^1.9.0",
        "chalk": "^4.1.1",
        "columnify": "^1.5.4",
        "commander": "^8.1.0",
        "cross-spawn": "7.0.6",
        "dotenv": "16.4.5",
        "fast-glob": "3.3.3",
        "fs-extra": "^10.0.0",
        "inquirer": "^8.2.3",
        "lodash-es": "^4.17.23",
        "ora": "^5.3.0",
        "strip-ansi": "^6.0.0",
        "tar": "^6.2.1",
        "tslib": "2.8.1",
        "validate-npm-package-name": "^5.0.0"
    },
    "devDependencies": {
        "@gooddata/eslint-config": "workspace:*",
        "@gooddata/oxlint-config": "workspace:*",
        "@types/columnify": "^1.5.1",
        "@types/cross-spawn": "^6.0.2",
        "@types/fs-extra": "^9.0.13",
        "@types/inquirer": "^8.2.6",
        "@types/lodash-es": "^4.17.12",
        "@types/node": "22.13.0",
        "@types/tar": "^6.1.0",
        "@typescript-eslint/eslint-plugin": "8.52.0",
        "@typescript-eslint/parser": "8.52.0",
        "@typescript/native-preview": "7.0.0-dev.20260202.1",
        "dependency-cruiser": "^14.1.2",
        "eslint": "^9.39.2",
        "eslint-import-resolver-typescript": "4.4.4",
        "eslint-plugin-headers": "1.3.3",
        "eslint-plugin-import-esm": "1.2.1",
        "eslint-plugin-import-x": "4.16.1",
        "eslint-plugin-jsdoc": "62.1.0",
        "eslint-plugin-no-barrel-files": "1.2.2",
        "eslint-plugin-sonarjs": "3.0.6",
        "oxfmt": "0.27.0",
        "oxlint": "^1.43.0",
        "oxlint-tsgolint": "0.11.4",
        "typescript": "5.9.3",
        "vitest": "4.0.8"
    }
}
