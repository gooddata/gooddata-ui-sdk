{
    "name": "@gooddata/catalog-export",
    "version": "8.0.0-alpha.101",
    "author": "GoodData",
    "description": "GoodData SDK Catalog Export tooling",
    "repository": "https://github.com/gooddata/gooddata-ui-sdk/tree/master/tools/catalog-export",
    "license": "BSD-3-Clause",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "bin": {
        "gdc-catalog-export": "dist/index.js"
    },
    "files": [
        "dist",
        "src",
        "LICENSE"
    ],
    "config": {
        "tslint": "./{src,test}/**/*.ts"
    },
    "scripts": {
        "clean": "rm -rf ci dist coverage *.log && jest --clearCache",
        "dev": "tsc -p tsconfig.dev.json --watch",
        "build": "rm -rf dist && tsc -p tsconfig.build.json",
        "test": "jest --watch",
        "test-once": "jest",
        "test-ci": "JEST_JUNIT_OUTPUT=./ci/results/test-results.xml jest --ci --config jest.ci.js",
        "tslint": "tslint -p . -t verbose $npm_package_config_tslint",
        "tslint-ci": "mkdir -p ./ci/results && tslint -p . -t checkstyle -o ./ci/results/tslint-results.xml $npm_package_config_tslint",
        "prettier-check": "prettier --check '{src,test}/**/*.{ts,tsx,json,scss,md,yaml,html}'",
        "prettier-write": "prettier --write '{src,test}/**/*.{ts,tsx,json,scss,md,yaml,html}'",
        "dep-cruiser": "depcruise --validate .dependency-cruiser.js --output-type err-long src/",
        "dep-cruiser-ci": "mkdir -p ci/results && depcruise --validate .dependency-cruiser.js --output-type err-html src/ >./ci/results/dep-cruiser.html",
        "validate": "npm run dep-cruiser && npm run tslint && npm run prettier-check",
        "validate-ci": "npm run dep-cruiser-ci && npm run tslint-ci && npm run prettier-check"
    },
    "dependencies": {
        "@babel/core": "^7.7.2",
        "@babel/plugin-transform-typescript": "^7.7.2",
        "@gooddata/gd-bear-client": "^8.0.0-alpha.101",
        "@gooddata/gd-bear-model": "^8.0.0-alpha.101",
        "@gooddata/gd-tiger-client": "^8.0.0-alpha.101",
        "chalk": "^2.4.2",
        "commander": "^3.0.2",
        "inquirer": "^6.5.2",
        "lodash": "^4.17.15",
        "ora": "^4.0.2",
        "p-map": "^3.0.0",
        "prettier": "~2.0.5",
        "prompt": "^1.0.0",
        "strip-ansi": "^5.2.0",
        "ts-morph": "^4.2.0",
        "tslib": "^2.0.0"
    },
    "devDependencies": {
        "@gooddata/tslint-config": "1.0.0",
        "@types/inquirer": "^6.5.0",
        "@types/jest": "^25.1.2",
        "@types/lodash": "^4.14.123",
        "@types/node": "^12.12.26",
        "@types/prettier": "~1.18.2",
        "dependency-cruiser": "^8.0.1",
        "jest": "^25.1.0",
        "jest-junit": "^3.0.0",
        "prettier": "~2.0.5",
        "ts-jest": "^25.2.0",
        "ts-node": "^8.4.1",
        "tslint": "^5.14.0",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "3.9.5"
    }
}
