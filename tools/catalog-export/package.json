{
    "name": "@gooddata/catalog-export",
    "version": "11.20.0-alpha.1",
    "description": "GoodData SDK Catalog Export tooling",
    "license": "MIT",
    "author": "GoodData",
    "repository": {
        "type": "git",
        "url": "https://github.com/gooddata/gooddata-ui-sdk.git",
        "directory": "tools/catalog-export"
    },
    "bin": {
        "gdc-catalog-export": "./esm/index.js"
    },
    "files": [
        "esm/**/*.js",
        "esm/**/*.json",
        "esm/**/*.d.ts",
        "esm/**/*.map"
    ],
    "type": "module",
    "types": "./esm/index.d.ts",
    "exports": "./esm/index.js",
    "scripts": {
        "_phase:build": "npm run build",
        "_phase:post-build": "npm run build-dynamic-files",
        "_phase:test": "npm run build-dynamic-files && npm run test-once",
        "_phase:validate": "npm run build-dynamic-files && npm run validate",
        "build": "bash scripts/build.sh",
        "build-check": "tsgo",
        "build-ts": "tsgo -p tsconfig.build.json",
        "build-dynamic-files": "bash scripts/build.sh --genFilesOnly",
        "clean": "../../common/scripts/clean-command-state.sh && rm -rf ci dist esm coverage *.log tsconfig.tsbuildinfo",
        "dep-cruiser": "depcruise --validate .dependency-cruiser.cjs --output-type err-long src/",
        "format-check": "oxfmt --check .",
        "format-write": "oxfmt .",
        "test": "vitest watch",
        "test-once": "vitest run",
        "validate": "npm run dep-cruiser && npm run lint && npm run format-check",
        "lint": "oxlint . --type-aware && eslint src",
        "lint-fix": "oxlint . --type-aware --fix && eslint src --fix"
    },
    "dependencies": {
        "@babel/core": "^7.23.5",
        "@babel/plugin-transform-typescript": "^7.23.5",
        "@gooddata/api-client-tiger": "workspace:*",
        "@gooddata/sdk-model": "workspace:*",
        "chalk": "^4.1.1",
        "commander": "^8.1.0",
        "dotenv": "16.0.3",
        "inquirer": "^8.2.3",
        "lodash-es": "^4.17.23",
        "open": "^7.4.0",
        "ora": "^5.3.0",
        "oxfmt": "0.27.0",
        "p-map": "^3.0.0",
        "strip-ansi": "^6.0.0",
        "ts-morph": "^26.0.0",
        "tslib": "2.8.1"
    },
    "devDependencies": {
        "@gooddata/eslint-config": "workspace:*",
        "@gooddata/oxlint-config": "workspace:*",
        "@types/babel__core": "^7.20.1",
        "@types/inquirer": "^8.2.6",
        "@types/lodash-es": "^4.17.12",
        "@types/node": "^22.13.0",
        "@typescript-eslint/eslint-plugin": "8.52.0",
        "@typescript-eslint/parser": "8.52.0",
        "@typescript/native-preview": "7.0.0-dev.20260202.1",
        "dependency-cruiser": "^14.1.2",
        "eslint": "^9.39.2",
        "eslint-import-resolver-typescript": "4.4.4",
        "eslint-plugin-headers": "1.3.3",
        "eslint-plugin-import-esm": "1.2.1",
        "eslint-plugin-import-x": "4.16.1",
        "eslint-plugin-jsdoc": "62.1.0",
        "eslint-plugin-no-barrel-files": "1.2.2",
        "eslint-plugin-sonarjs": "3.0.6",
        "happy-dom": "18.0.1",
        "npm-run-all": "^4.1.5",
        "oxlint": "^1.43.0",
        "oxlint-tsgolint": "0.11.4",
        "typescript": "5.9.3",
        "vitest": "4.0.8"
    }
}
