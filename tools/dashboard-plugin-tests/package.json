{
    "name": "@gooddata/dashboard-plugin-tests",
    "version": "1.0.0",
    "description": "Application and servers for dashboard plugins API testing",
    "repository": {
        "type": "git",
        "url": "https://github.com/gooddata/gooddata-ui-sdk.git",
        "directory": "tools/dashboard-plugin-tests"
    },
    "author": "GoodData Corporation",
    "license": "LicenseRef-LICENSE",
    "main": "dist/index.js",
    "module": "esm/index.js",
    "es2015": "esm/index.js",
    "browser": "dist/index.js",
    "typings": "esm/index.d.ts",
    "sideEffects": [
        "*.css",
        "*.svg"
    ],
    "files": [
        "dist/**/*.js",
        "dist/**/*.json",
        "dist/**/*.d.ts",
        "dist/**/*.map",
        "dist/**/*.svg",
        "esm/**/*.js",
        "esm/**/*.json",
        "esm/**/*.d.ts",
        "esm/**/*.map",
        "esm/**/*.svg",
        "styles"
    ],
    "config": {
        "eslint": "-c .eslintrc.js --ext ts,tsx cypress/ plugins/ plugins-loader/"
    },
    "scripts": {
        "clean": "rm -rf ci dist esm coverage *.log styles/css && jest --clearCache",
        "test": "jest --watch",
        "test-once": "jest --passWithNoTests --maxWorkers=${JEST_MAX_WORKERS:-'45%'}",
        "test-ci": "JEST_JUNIT_OUTPUT=./ci/results/test-results.xml jest --ci --config jest.ci.js --passWithNoTests",
        "eslint": "eslint $npm_package_config_eslint",
        "eslint-ci": "mkdir -p ./ci/results && eslint -f checkstyle -o ci/results/eslint-results.xml $npm_package_config_eslint",
        "prettier-check": "prettier --check '{src,stories,styles,__mocks__}/**/*.{ts,tsx,json,scss,md,yaml,html}'",
        "prettier-write": "prettier --write '{src,stories,styles,__mocks__}/**/*.{ts,tsx,json,scss,md,yaml,html}'",
        "dep-cruiser": "depcruise --validate .dependency-cruiser.js --output-type err-long cypress/ plugins/ plugins-loader/",
        "dep-cruiser-ci": "depcruise --validate .dependency-cruiser.js --output-type err-long cypress/ plugins/ plugins-loader/",
        "validate": "npm run dep-cruiser && npm run eslint && npm run prettier-check",
        "validate-ci": "npm run dep-cruiser-ci && npm run eslint-ci && npm run prettier-check",
        "install-plugins": "bash ./scripts/install-plugins.sh",
        "build": "npm run build-plugins-loader && npm run install-plugins && npm run build-plugins",
        "build-plugins": "bash ./scripts/build-plugins.sh",
        "build-plugins-loader": "bash ./scripts/build-plugins-loader.sh",
        "cypress": "node ./scripts/run-cypress-local.js",
        "docker-build": "npm run build && DOCKER_BUILDKIT=0 docker build --file Dockerfile -t dashboard-plugin-tests .",
        "docker-build-local": "docker build --file Dockerfile_local -t dashboard-plugin-tests .",
        "run-isolated": "docker-compose -f docker-compose-isolated.yaml up --abort-on-container-exit --exit-code-from isolated-tests --force-recreate --always-recreate-deps --renew-anon-volumes",
        "run-isolated-record": "MODE=record npm run run-isolated-local",
        "run-isolated-local": "npm run docker-build-local && IMAGE_ID=dashboard-plugin-tests npm run run-isolated",
        "run-isolated-ci": "npm run docker-build && IMAGE_ID=dashboard-plugin-tests npm run run-isolated",
        "run-plugins-local": "node ./scripts/run-plugins-local.js",
        "refresh-md": "./scripts/refresh-md.sh"
    },
    "dependencies": {
        "@babel/polyfill": "^7.7.2",
        "@gooddata/api-model-bear": "^8.12.3-beta.0",
        "@gooddata/catalog-export": "^8.12.3-beta.0",
        "@gooddata/sdk-backend-base": "^8.12.3-beta.0",
        "@gooddata/sdk-backend-bear": "^8.12.3-beta.0",
        "@gooddata/sdk-backend-spi": "^8.12.3-beta.0",
        "@gooddata/sdk-model": "^8.12.3-beta.0",
        "@gooddata/sdk-ui": "^8.12.3-beta.0",
        "@gooddata/sdk-ui-charts": "^8.12.3-beta.0",
        "@gooddata/sdk-ui-dashboard": "^8.12.3-beta.0",
        "@gooddata/sdk-ui-ext": "^8.12.3-beta.0",
        "@gooddata/sdk-ui-filters": "^8.12.3-beta.0",
        "@gooddata/sdk-ui-geo": "^8.12.3-beta.0",
        "@gooddata/sdk-ui-kit": "^8.12.3-beta.0",
        "@gooddata/sdk-ui-loaders": "^8.12.3-beta.0",
        "@gooddata/sdk-ui-pivot": "^8.12.3-beta.0",
        "@gooddata/sdk-ui-theme-provider": "^8.12.3-beta.0",
        "axios": "^0.21.4",
        "dotenv": "^10.0.0",
        "lodash": "^4.17.19",
        "react-intl": "^5.23.0",
        "tslib": "^2.0.0",
        "wait-on": "^6.0.0",
        "xml2js": "^0.4.23"
    },
    "peerDependencies": {
        "react": "^16.10.0 || ^17.0.0",
        "react-dom": "^16.10.0 || ^17.0.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.7.2",
        "@babel/core": "^7.7.2",
        "@babel/plugin-proposal-class-properties": "^7.7.2",
        "@babel/plugin-proposal-object-rest-spread": "^7.7.2",
        "@babel/plugin-transform-async-to-generator": "^7.7.2",
        "@babel/plugin-transform-typescript": "^7.7.2",
        "@babel/preset-env": "^7.7.2",
        "@babel/preset-react": "^7.7.2",
        "@babel/preset-typescript": "^7.7.2",
        "@babel/runtime": "^7.7.2",
        "@gooddata/eslint-config": "^4.1.0",
        "@microsoft/api-documenter": "^7.17.0",
        "@microsoft/api-extractor": "^7.20.0",
        "@types/enzyme": "^3.10.3",
        "@types/jest": "^27.0.1",
        "@types/lodash": "^4.14.158",
        "@types/node": "^16.11.11",
        "@types/raf": "^3.4.0",
        "@types/react-dom": "^17.0.11",
        "@types/react": "^17.0.34",
        "@types/wait-on": "^5.3.1",
        "@types/xml2js": "^0.4.9",
        "@typescript-eslint/eslint-plugin": "^5.45.0",
        "@typescript-eslint/parser": "^5.45.0",
        "@wojtekmaj/enzyme-adapter-react-17": "^0.6.5",
        "babel-loader": "^8.0.5",
        "circular-dependency-plugin": "^5.2.0",
        "clean-webpack-plugin": "^4.0.0",
        "compression-webpack-plugin": "^7.1.0",
        "concurrently": "^6.0.2",
        "css-loader": "^6.7.1",
        "cypress": "^9.7.0",
        "dependency-cruiser": "^12.12.2",
        "dotenv-webpack": "^7.0.2",
        "enzyme": "^3.10.0",
        "eslint": "^8.25.0",
        "eslint-plugin-header": "^3.0.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-jest": "^27.1.1",
        "eslint-plugin-no-only-tests": "^2.4.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react-hooks": "^4.5.0",
        "eslint-plugin-react": "^7.31.10",
        "eslint-plugin-regexp": "^1.9.0",
        "eslint-plugin-sonarjs": "^0.16.0",
        "fork-ts-checker-webpack-plugin": "^7.2.13",
        "html-webpack-plugin": "^5.3.1",
        "jest": "^27.5.1",
        "jest-enzyme": "^7.1.2",
        "jest-junit": "^3.0.0",
        "prettier": "~2.5.0",
        "raf": "^3.4.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "source-map-loader": "^4.0.0",
        "speed-measure-webpack-plugin": "^1.5.0",
        "style-loader": "^3.3.1",
        "ts-jest": "^27.0.5",
        "ts-node": "^10.4.0",
        "typescript": "4.0.2",
        "webpack": "^5.58.0"
    }
}
