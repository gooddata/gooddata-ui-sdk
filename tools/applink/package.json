{
    "name": "@gooddata/applink",
    "version": "1.0.0",
    "description": "GoodData SDK Application Linking Tool",
    "repository": {
        "type": "git",
        "url": "https://github.com/gooddata/gooddata-ui-sdk.git",
        "directory": "tools/applink"
    },
    "license": "MIT",
    "author": "GoodData",
    "sideEffects": false,
    "type": "module",
    "exports": "./esm/index.js",
    "typings": "./esm/index.d.ts",
    "bin": {
        "gdc-applink": "esm/index.js"
    },
    "files": [
        "esm/**/*.js",
        "esm/**/*.json",
        "esm/**/*.d.ts",
        "esm/**/*.map"
    ],
    "scripts": {
        "_phase:build": "npm run build",
        "_phase:test": "npm run test-once",
        "_phase:validate": "npm run validate",
        "applink": "node esm/index.js $*",
        "build": "tsc -p tsconfig.build.json",
        "clean": "../../common/scripts/clean-command-state.sh && rm -rf ci dist esm coverage *.log tsconfig.tsbuildinfo",
        "dep-cruiser": "depcruise --validate .dependency-cruiser.cjs --output-type err-long src/",
        "eslint": "eslint src",
        "eslint-fix": "eslint src --fix",
        "prettier-check": "prettier --check .",
        "prettier-write": "prettier --write .",
        "test": "vitest watch",
        "test-once": "vitest run",
        "validate": "tsc && npm run dep-cruiser && npm run eslint && npm run prettier-check"
    },
    "devDependencies": {
        "@gooddata/eslint-config": "workspace:*",
        "@types/blessed": "^0.1.17",
        "@types/cross-spawn": "^6.0.2",
        "@types/json5": "^0.0.30",
        "@types/lodash-es": "^4.17.12",
        "@types/node": "^22.13.0",
        "@types/watchpack": "^2.4.4",
        "@typescript-eslint/eslint-plugin": "8.52.0",
        "@typescript-eslint/parser": "8.52.0",
        "blessed": "^0.1.81",
        "commander": "^8.1.0",
        "cross-spawn": "7.0.6",
        "dependency-cruiser": "^14.1.2",
        "eslint": "^9.39.2",
        "eslint-plugin-header": "3.1.1",
        "eslint-plugin-import-esm": "1.2.1",
        "eslint-plugin-import-x": "4.16.1",
        "eslint-plugin-jsdoc": "62.1.0",
        "eslint-plugin-no-barrel-files": "1.2.2",
        "eslint-plugin-no-only-tests": "3.3.0",
        "eslint-plugin-sonarjs": "3.0.6",
        "find-up": "^5.0.0",
        "json5": "^2.1.3",
        "lodash-es": "^4.17.23",
        "prettier": "^3.6.2",
        "tslib": "2.8.1",
        "typescript": "5.9.3",
        "vitest": "4.0.8",
        "watchpack": "^2.4.4"
    },
    "dependenciesInfo": "This is package is SDK tool and is not distributed all dependencies must be defined as devDependencies due to FOSSA automated report generation"
}
