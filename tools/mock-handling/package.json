{
    "name": "@gooddata/mock-handling",
    "version": "11.19.0-alpha.3",
    "description": "GoodData SDK Mock data capture and management tool",
    "repository": {
        "type": "git",
        "url": "https://github.com/gooddata/gooddata-ui-sdk.git",
        "directory": "tools/mock-handling"
    },
    "license": "MIT",
    "author": "GoodData",
    "type": "module",
    "exports": "./esm/interface.js",
    "types": "./esm/interface.d.ts",
    "bin": {
        "gdc-mock-handling": "esm/index.js"
    },
    "files": [
        "esm/**/*.js",
        "esm/**/*.json",
        "esm/**/*.d.ts",
        "esm/**/*.map"
    ],
    "scripts": {
        "_phase:build": "npm run build",
        "_phase:post-build": "npm run build-dynamic-files",
        "_phase:test": "npm run build-dynamic-files && npm run test-once",
        "_phase:validate": "npm run build-dynamic-files && npm run validate",
        "build": "bash scripts/build.sh",
        "build-dynamic-files": "bash scripts/build.sh --genFilesOnly",
        "clean": "../../common/scripts/clean-command-state.sh && rm -rf ci dist esm coverage *.log tsconfig.tsbuildinfo",
        "dep-cruiser": "depcruise --validate .dependency-cruiser.cjs --output-type err-long src/",
        "eslint": "eslint src",
        "eslint-fix": "eslint src --fix",
        "prettier-check": "prettier --no-error-on-unmatched-pattern --check '{src,test}/**/*.{json,scss,md,yaml,html}'",
        "prettier-write": "prettier --no-error-on-unmatched-pattern --write '{src,test}/**/*.{json,scss,md,yaml,html}'",
        "test": "vitest watch",
        "test-once": "vitest run",
        "validate": "tsc && npm run dep-cruiser && npm run eslint && npm run prettier-check",
        "validate-esm": "node --input-type=module --eval 'import \"@gooddata/mock-handling\"'"
    },
    "dependencies": {
        "@gooddata/sdk-backend-spi": "workspace:*",
        "@gooddata/sdk-backend-tiger": "workspace:*",
        "@gooddata/sdk-model": "workspace:*",
        "chalk": "^4.1.1",
        "commander": "^8.1.0",
        "inquirer": "^8.2.3",
        "json-stable-stringify": "1.3.0",
        "lodash-es": "^4.17.21",
        "ora": "^5.3.0",
        "p-map": "^3.0.0",
        "prettier": "^3.6.2",
        "tslib": "2.8.1"
    },
    "devDependencies": {
        "@gooddata/eslint-config": "workspace:*",
        "@types/inquirer": "^8.2.6",
        "@types/lodash-es": "^4.17.12",
        "@types/node": "^22.13.0",
        "@types/prettier": "~1.18.2",
        "@typescript-eslint/eslint-plugin": "8.52.0",
        "@typescript-eslint/parser": "8.52.0",
        "dependency-cruiser": "^14.1.2",
        "eslint": "^9.39.2",
        "eslint-config-prettier": "10.1.8",
        "eslint-plugin-header": "3.1.1",
        "eslint-plugin-import-esm": "1.2.1",
        "eslint-plugin-import-x": "4.16.1",
        "eslint-plugin-jsdoc": "62.1.0",
        "eslint-plugin-no-barrel-files": "1.2.2",
        "eslint-plugin-no-only-tests": "3.3.0",
        "eslint-plugin-prettier": "5.5.4",
        "eslint-plugin-sonarjs": "3.0.6",
        "typescript": "5.9.3",
        "vitest": "4.0.8"
    }
}
