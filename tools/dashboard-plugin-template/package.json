{
    "name": "@gooddata/dashboard-plugin-template",
    "version": "1.0.0",
    "private": true,
    "description": "GoodData Template for Dashboard Plugins",
    "license": "LicenseRef-LICENSE",
    "author": "GoodData Corporation",
    "repository": {
        "type": "git",
        "url": "https://github.com/gooddata/gooddata-ui-sdk.git",
        "directory": "tools/dashboard-plugin-template"
    },
    "files": [
        "esm/**/*.js",
        "esm/**/*.json",
        "esm/**/*.d.ts",
        "esm/**/*.map",
        "esm/**/*.svg"
    ],
    "type": "module",
    "sideEffects": [
        "*.css",
        "*.svg"
    ],
    "types": "esm/index.d.ts",
    "exports": "./esm/index.js",
    "scripts": {
        "_phase:build": "npm run build",
        "_phase:test": "npm run test-once",
        "_phase:validate": "npm run validate",
        "add-plugin": "gdc-plugins dashboard-plugin add",
        "build": "webpack --mode production --config-name harness",
        "build-plugin": "webpack --mode production --config-name dashboardPlugin",
        "build-plugin-debug": "webpack --mode production --config-name dashboardPlugin --analyze",
        "clean": "../../common/scripts/clean-command-state.sh && rm -rf ci dist esm coverage *.log styles/css tsconfig.tsbuildinfo",
        "dep-cruiser": "depcruise --validate .dependency-cruiser.jsc --output-type err-long src/",
        "eslint": "eslint src",
        "eslint-fix": "eslint src --fix",
        "gdc-plugins": "gdc-plugins",
        "link-plugin": "gdc-plugins dashboard-plugin link",
        "oxfmt-check": "oxfmt --check .",
        "oxfmt-write": "oxfmt .",
        "refresh-md": "node ./scripts/refresh-md.js",
        "remove-plugin-params": "gdc-plugins dashboard-plugin remove-params",
        "start": "webpack-cli serve --config-name harness",
        "test": "vitest --watch --passWithNoTests",
        "test-once": "vitest run --passWithNoTests",
        "unlink-plugin": "gdc-plugins dashboard-plugin unlink",
        "update-plugin-params": "gdc-plugins dashboard-plugin update-params",
        "validate": "tsgo && npm run dep-cruiser && npm run eslint && npm run oxfmt-check"
    },
    "dependencies": {
        "json-stable-stringify": "1.3.0",
        "lodash-es": "^4.17.23",
        "tslib": "2.8.1"
    },
    "devDependencies": {
        "@babel/core": "^7.23.5",
        "@babel/preset-env": "^7.23.5",
        "@babel/preset-react": "^7.23.3",
        "@gooddata/catalog-export": "workspace:*",
        "@gooddata/eslint-config": "workspace:*",
        "@gooddata/plugin-toolkit": "workspace:*",
        "@gooddata/sdk-backend-spi": "workspace:*",
        "@gooddata/sdk-backend-tiger": "workspace:*",
        "@gooddata/sdk-model": "workspace:*",
        "@gooddata/sdk-ui": "workspace:*",
        "@gooddata/sdk-ui-charts": "workspace:*",
        "@gooddata/sdk-ui-dashboard": "workspace:*",
        "@gooddata/sdk-ui-ext": "workspace:*",
        "@gooddata/sdk-ui-filters": "workspace:*",
        "@gooddata/sdk-ui-geo": "workspace:*",
        "@gooddata/sdk-ui-kit": "workspace:*",
        "@gooddata/sdk-ui-loaders": "workspace:*",
        "@gooddata/sdk-ui-pivot": "workspace:*",
        "@types/lodash-es": "^4.17.12",
        "@types/node": "^22.13.0",
        "@types/raf": "^3.4.0",
        "@types/react": "19.1.11",
        "@types/react-dom": "19.1.7",
        "@typescript-eslint/eslint-plugin": "8.52.0",
        "@typescript-eslint/parser": "8.52.0",
        "@typescript/native-preview": "7.0.0-dev.20260202.1",
        "babel-loader": "^8.0.5",
        "case-sensitive-paths-webpack-plugin": "^2.4.0",
        "css-loader": "^6.7.1",
        "dependency-cruiser": "^14.1.2",
        "dotenv": "16.0.3",
        "dotenv-webpack": "^7.0.2",
        "esbuild-loader": "^4.0.2",
        "eslint": "^9.39.2",
        "eslint-import-resolver-typescript": "4.4.4",
        "eslint-plugin-header": "3.1.1",
        "eslint-plugin-import-esm": "1.2.1",
        "eslint-plugin-import-x": "4.16.1",
        "eslint-plugin-jsdoc": "62.1.0",
        "eslint-plugin-no-barrel-files": "1.2.2",
        "eslint-plugin-no-only-tests": "3.3.0",
        "eslint-plugin-react": "7.37.5",
        "eslint-plugin-react-hooks": "5.2.0",
        "eslint-plugin-sonarjs": "3.0.6",
        "html-webpack-plugin": "^5.3.1",
        "lodash-es": "^4.17.23",
        "oxfmt": "0.27.0",
        "raf": "^3.4.1",
        "react": "19.1.1",
        "react-dom": "19.1.1",
        "react-intl": "7.1.11",
        "source-map-loader": "^4.0.0",
        "style-loader": "^3.3.1",
        "ts-loader": "^8.3.0",
        "typescript": "5.9.3",
        "vitest": "4.0.8",
        "webpack": "5.93.0",
        "webpack-bundle-analyzer": "^4.5.0",
        "webpack-cli": "6.0.1",
        "webpack-dev-server": "5.2.2"
    },
    "peerDependencies": {
        "@gooddata/sdk-backend-spi": "workspace:*",
        "@gooddata/sdk-backend-tiger": "workspace:*",
        "@gooddata/sdk-model": "workspace:*",
        "@gooddata/sdk-ui": "workspace:*",
        "@gooddata/sdk-ui-charts": "workspace:*",
        "@gooddata/sdk-ui-dashboard": "workspace:*",
        "@gooddata/sdk-ui-ext": "workspace:*",
        "@gooddata/sdk-ui-filters": "workspace:*",
        "@gooddata/sdk-ui-geo": "workspace:*",
        "@gooddata/sdk-ui-kit": "workspace:*",
        "@gooddata/sdk-ui-pivot": "workspace:*",
        "react": "^18.0.0",
        "react-dom": "^18.0.0"
    }
}
