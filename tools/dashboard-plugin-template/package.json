{
    "name": "@gooddata/dashboard-plugin-template",
    "version": "1.0.0",
    "description": "GoodData Template for Dashboard Plugins",
    "repository": {
        "type": "git",
        "url": "https://github.com/gooddata/gooddata-ui-sdk.git",
        "directory": "tools/dashboard-plugin-template"
    },
    "author": "GoodData Corporation",
    "private": true,
    "license": "MIT",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "sideEffects": [
        "*.css",
        "*.svg"
    ],
    "files": [
        "dist/**/*.js",
        "dist/**/*.json",
        "dist/**/*.d.ts",
        "dist/**/*.map",
        "dist/**/*.svg",
        "styles"
    ],
    "config": {
        "eslint": "-c .eslintrc.js --ext ts,tsx src/"
    },
    "scripts": {
        "clean": "rm -rf ci dist esm coverage *.log styles/css && jest --clearCache",
        "build": "webpack --mode production --config-name harness",
        "build-plugin": "webpack --mode production --config-name dashboardPlugin",
        "start": "webpack-cli serve --config-name harness",
        "test": "jest --watch --passWithNoTests",
        "test-once": "jest --passWithNoTests",
        "test-ci": "JEST_JUNIT_OUTPUT=./ci/results/test-results.xml jest --ci --config jest.ci.js --passWithNoTests",
        "eslint": "eslint $npm_package_config_eslint",
        "eslint-ci": "mkdir -p ./ci/results && eslint -f checkstyle -o ci/results/eslint-results.xml $npm_package_config_eslint",
        "prettier-check": "prettier --check '{src,stories,styles,__mocks__}/**/*.{ts,tsx,json,scss,md,yaml,html}'",
        "prettier-write": "prettier --write '{src,stories,styles,__mocks__}/**/*.{ts,tsx,json,scss,md,yaml,html}'",
        "dep-cruiser": "depcruise --validate .dependency-cruiser.js --output-type err-long src/",
        "dep-cruiser-ci": "depcruise --validate .dependency-cruiser.js --output-type err-long src/",
        "validate": "npm run dep-cruiser && npm run eslint && npm run prettier-check",
        "validate-ci": "npm run dep-cruiser-ci && npm run eslint-ci && npm run prettier-check"
    },
    "dependencies": {
        "@gooddata/sdk-backend-bear": "^8.6.0-alpha.41",
        "@gooddata/sdk-backend-spi": "^8.6.0-alpha.41",
        "@gooddata/sdk-model": "^8.6.0-alpha.41",
        "@gooddata/sdk-ui": "^8.6.0-alpha.41",
        "@gooddata/sdk-ui-ext": "^8.6.0-alpha.41",
        "lodash": "^4.17.19",
        "react": "^16.10.0",
        "react-dom": "^16.10.0",
        "react-intl": "^3.6.0",
        "process": "^0.11.10",
        "tslib": "^2.0.0",
        "util": "^0.12.3"
    },
    "devDependencies": {
        "@babel/core": "^7.7.2",
        "@babel/preset-env": "^7.7.2",
        "@babel/preset-react": "^7.7.2",
        "@gooddata/eslint-config": "^2.1.0",
        "@types/enzyme-adapter-react-16": "^1.0.5",
        "@types/enzyme": "^3.10.3",
        "@types/jest": "^26.0.12",
        "@types/lodash": "^4.14.158",
        "@types/raf": "^3.4.0",
        "@types/react-dom": "^16.9.3",
        "@types/react": "^16.9.11",
        "@typescript-eslint/eslint-plugin": "^4.14.2",
        "@typescript-eslint/parser": "^4.14.2",
        "babel-loader": "^8.0.5",
        "case-sensitive-paths-webpack-plugin": "^2.4.0",
        "concurrently": "^6.0.2",
        "css-loader": "^5.2.4",
        "dependency-cruiser": "^9.9.5",
        "enzyme": "^3.10.0",
        "enzyme-adapter-react-16": "^1.15.1",
        "eslint": "^7.5.0",
        "eslint-plugin-header": "^3.0.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-jest": "^24.1.3",
        "eslint-plugin-no-only-tests": "^2.4.0",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-react-hooks": "^4.0.8",
        "eslint-plugin-react": "^7.20.5",
        "eslint-plugin-sonarjs": "^0.9.1",
        "jest": "^26.4.2",
        "jest-enzyme": "^7.1.2",
        "jest-junit": "^3.0.0",
        "prettier": "~2.3.0",
        "raf": "^3.4.1",
        "source-map-loader": "^2.0.2",
        "style-loader": "^1.2.1",
        "ts-jest": "^26.3.0",
        "ts-loader": "^8.3.0",
        "typescript": "4.0.2",
        "webpack": "^5.18.0",
        "webpack-cli": "^4.6.0",
        "webpack-dev-server": "^3.11.2"
    }
}
