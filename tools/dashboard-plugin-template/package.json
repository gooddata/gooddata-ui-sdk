{
    "name": "@gooddata/dashboard-plugin-template",
    "version": "1.0.0",
    "description": "GoodData Template for Dashboard Plugins",
    "repository": {
        "type": "git",
        "url": "https://github.com/gooddata/gooddata-ui-sdk.git",
        "directory": "tools/dashboard-plugin-template"
    },
    "author": "GoodData Corporation",
    "private": true,
    "license": "LicenseRef-LICENSE",
    "type": "module",
    "exports": "./esm/index.js",
    "types": "esm/index.d.ts",
    "sideEffects": [
        "*.css",
        "*.svg"
    ],
    "files": [
        "esm/**/*.js",
        "esm/**/*.json",
        "esm/**/*.d.ts",
        "esm/**/*.map",
        "esm/**/*.svg"
    ],
    "scripts": {
        "clean": "rm -rf ci dist esm coverage *.log styles/css tsconfig.tsbuildinfo",
        "build": "webpack --mode production --config-name harness",
        "build-plugin": "webpack --mode production --config-name dashboardPlugin",
        "build-plugin-debug": "webpack --mode production --config-name dashboardPlugin --analyze",
        "gdc-plugins": "gdc-plugins",
        "add-plugin": "gdc-plugins dashboard-plugin add",
        "link-plugin": "gdc-plugins dashboard-plugin link",
        "unlink-plugin": "gdc-plugins dashboard-plugin unlink",
        "update-plugin-params": "gdc-plugins dashboard-plugin update-params",
        "remove-plugin-params": "gdc-plugins dashboard-plugin remove-params",
        "start": "webpack-cli serve --config-name harness",
        "test": "vitest --watch --passWithNoTests",
        "test-once": "vitest --passWithNoTests",
        "test-ci": "vitest run --reporter=junit --outputFile=./ci/results/test-results.xml --passWithNoTests",
        "eslint": "eslint -c .eslintrc.cjs src/",
        "eslint-ci": "mkdir -p ./ci/results && eslint -f checkstyle -o ci/results/eslint-results.xml -c .eslintrc.cjs src/",
        "prettier-check": "prettier --check \"{src,test}/**/*.{ts,tsx,json,scss,md,yaml,html}\"",
        "prettier-write": "prettier --write \"{src,test}/**/*.{ts,tsx,json,scss,md,yaml,html}\"",
        "dep-cruiser": "depcruise --validate .dependency-cruiser.jsc --output-type err-long src/",
        "dep-cruiser-ci": "depcruise --validate .dependency-cruiser.jsc --output-type err-long src/",
        "validate": "npm run dep-cruiser && npm run eslint && npm run prettier-check",
        "validate-ci": "npm run dep-cruiser-ci && npm run eslint-ci && npm run prettier-check",
        "refresh-md": "node ./scripts/refresh-md.js"
    },
    "dependencies": {
        "json-stable-stringify": "^1.0.1",
        "lodash": "^4.17.19",
        "tslib": "^2.5.0"
    },
    "peerDependencies": {
        "@gooddata/sdk-model": "workspace:*",
        "@gooddata/sdk-backend-spi": "workspace:*",
        "@gooddata/sdk-backend-tiger": "workspace:*",
        "@gooddata/sdk-ui": "workspace:*",
        "@gooddata/sdk-ui-dashboard": "workspace:*",
        "@gooddata/sdk-ui-charts": "workspace:*",
        "@gooddata/sdk-ui-ext": "workspace:*",
        "@gooddata/sdk-ui-filters": "workspace:*",
        "@gooddata/sdk-ui-geo": "workspace:*",
        "@gooddata/sdk-ui-kit": "workspace:*",
        "@gooddata/sdk-ui-pivot": "workspace:*",
        "react": "^16.10.0 || ^17.0.0 || ^18.0.0",
        "react-dom": "^16.10.0 || ^17.0.0 || ^18.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.23.5",
        "@babel/preset-env": "^7.23.5",
        "@babel/preset-react": "^7.23.3",
        "@gooddata/catalog-export": "workspace:*",
        "@gooddata/eslint-config": "^4.1.0",
        "@gooddata/plugin-toolkit": "workspace:*",
        "@gooddata/sdk-backend-spi": "workspace:*",
        "@gooddata/sdk-backend-tiger": "workspace:*",
        "@gooddata/sdk-model": "workspace:*",
        "@gooddata/sdk-ui": "workspace:*",
        "@gooddata/sdk-ui-dashboard": "workspace:*",
        "@gooddata/sdk-ui-charts": "workspace:*",
        "@gooddata/sdk-ui-ext": "workspace:*",
        "@gooddata/sdk-ui-filters": "workspace:*",
        "@gooddata/sdk-ui-geo": "workspace:*",
        "@gooddata/sdk-ui-kit": "workspace:*",
        "@gooddata/sdk-ui-loaders": "workspace:*",
        "@gooddata/sdk-ui-pivot": "workspace:*",
        "@types/json-stable-stringify": "^1.0.32",
        "@types/lodash": "^4.14.202",
        "@types/node": "^18.17.6",
        "@types/raf": "^3.4.0",
        "@types/react": "^18.2.0",
        "@types/react-dom": "^18.2.1",
        "@typescript-eslint/eslint-plugin": "^5.57.1",
        "@typescript-eslint/parser": "^5.57.1",
        "babel-loader": "^8.0.5",
        "case-sensitive-paths-webpack-plugin": "^2.4.0",
        "concurrently": "^6.0.2",
        "css-loader": "^6.7.1",
        "dependency-cruiser": "^13.0.3",
        "dotenv": "16.0.3",
        "dotenv-webpack": "^7.0.2",
        "esbuild-loader": "^4.0.2",
        "eslint": "^8.25.0",
        "eslint-plugin-header": "^3.0.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-import-esm": "^1.2.1",
        "eslint-plugin-jest": "^27.1.1",
        "eslint-plugin-no-only-tests": "^2.4.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.31.10",
        "eslint-plugin-react-hooks": "^4.5.0",
        "eslint-plugin-regexp": "^1.9.0",
        "eslint-plugin-sonarjs": "^0.16.0",
        "eslint-plugin-tsdoc": "^0.2.14",
        "html-webpack-plugin": "^5.3.1",
        "lodash": "^4.17.19",
        "prettier": "~2.5.0",
        "raf": "^3.4.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-intl": "^6.4.1",
        "source-map-loader": "^4.0.0",
        "style-loader": "^3.3.1",
        "ts-loader": "^8.3.0",
        "typescript": "5.3.3",
        "vitest": "1.0.4",
        "webpack": "^5.58.0",
        "webpack-bundle-analyzer": "^4.5.0",
        "webpack-cli": "^4.9.2",
        "webpack-dev-server": "^4.9.1"
    },
    "overrides": {
        "reselect": "4.1.5"
    }
}
